    <recordUpdates>
        <description>Upon the Industry Field being set to &apos;672249&apos; then the existing Restricted Industry field on the Lead / Account record is automatically updated to be &apos;CRB&apos;</description>
        <name>Update_Restricted_Industry</name>
        <label>Update Restricted Industry</label>
        <locationX>314</locationX>
        <locationY>551</locationY>
        <connector>
            <targetReference>Check_for_PEO_Leasing</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Industry</field>
            <operator>EqualTo</operator>
            <value>
                <stringValue>672249</stringValue>
            </value>
        </filters>
        <filters>
            <field>Restricted_Industry__c</field>
            <operator>NotEqualTo</operator>
            <value>
                <stringValue>CRB</stringValue>
            </value>
        </filters>
        <inputAssignments>
            <field>Restricted_Industry__c</field>
            <value>
                <stringValue>CRB</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
