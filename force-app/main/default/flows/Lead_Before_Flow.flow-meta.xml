<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>54.0</apiVersion>
    <decisions>
        <name>Check_for_PEO_Leasing</name>
        <label>Check for PEO Leasing</label>
        <locationX>314</locationX>
        <locationY>671</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>PEO_Criteria</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.PEOLeasing__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Current_Prior_Method__c</leftValueReference>
                <operator>Contains</operator>
                <rightValue>
                    <stringValue>PEO</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Check_PEO_Leasing</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>PEO Criteria Met</label>
        </rules>
        <rules>
            <name>PEO_No_Longer_Mer</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.PEOLeasing__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Current_Prior_Method__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>CurrentPriorMethodNotPEO</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Uncheck_PEO_Leasing</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>true</doesRequireRecordChangedToMeetCriteria>
            <label>PEO No Longer Met</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>CurrentPriorMethodNotPEO</name>
        <dataType>Boolean</dataType>
        <expression>NOT(CONTAINS(TEXT({!$Record.Current_Prior_Method__c}), &quot;PEO&quot;))</expression>
    </formulas>
    <formulas>
        <name>IsNew</name>
        <dataType>Boolean</dataType>
        <expression>IsNew()</expression>
    </formulas>
    <interviewLabel>Lead Before Flow {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Lead Before Flow</label>
