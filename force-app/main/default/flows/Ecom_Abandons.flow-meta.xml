    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <description>Update Lead</description>
        <name>Update_Lead</name>
        <label>Update Lead</label>
        <locationX>506</locationX>
        <locationY>408</locationY>
        <connector>
            <targetReference>Update_Referrals</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Id</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Lead__c</elementReference>
            </value>
        </filters>
        <filters>
            <field>IsConverted</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </filters>
        <filters>
            <field>OwnerId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>LeadEcomOwner</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Hold_Out__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>LatestCampaign__c</field>
            <value>
                <elementReference>Campaign</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Lead_Stage__c</field>
            <value>
                <stringValue>Auto MQL</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>OwnedByPrev__c</field>
            <value>
                <stringValue></stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>ProcessNotes__c</field>
            <value>
                <stringValue>{!$Flow.CurrentDateTime}: Assigning abandoned Ecom Lead to VSR </stringValue>
            </value>
        </inputAssignments>
        <object>Lead</object>
    </recordUpdates>
    <recordUpdates>
        <description>Referral related to Lead</description>
        <name>Update_Referrals</name>
        <label>Update Referrals</label>
        <locationX>506</locationX>
        <locationY>528</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>SourceLead__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.Lead__c</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Campaign__c</field>
            <value>
                <elementReference>Campaign</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>OwnerId</field>
            <value>
                <elementReference>Queue</elementReference>
            </value>
        </inputAssignments>
        <object>Referral__c</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <filterLogic>(1 OR 2) AND 3 AND 4 AND 5 AND 6</filterLogic>
        <filters>
