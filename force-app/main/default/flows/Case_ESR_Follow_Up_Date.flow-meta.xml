    <description>Populate the follow up date when the case status changes</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>CASE( MOD( TODAY()  - DATE( 1900, 1, 7 ), 7 ),
  0, TODAY() + 3, /* Sun */
  3, TODAY() + 2 + 3, /* Wed */
  4, TODAY() + 2 + 3, /* Thurs */
  5, TODAY() + 2 + 3, /* Fri */
  6, TODAY() + 1 + 3, /* Sat */
  TODAY() + 3 /* Default (Mon/Tue) */
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_Follow_Up_Date_c</name>
        <dataType>Date</dataType>
        <expression>CASE( MOD( TODAY()  - DATE( 1900, 1, 7 ), 7 ),
  0, TODAY() + 3, /* Sun */
  3, TODAY() + 2 + 3, /* Wed */
  4, TODAY() + 2 + 3, /* Thurs */
  5, TODAY() + 2 + 3, /* Fri */
  6, TODAY() + 1 + 3, /* Sat */
  TODAY() + 3 /* Default (Mon/Tue) */
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>CASE( MOD( TODAY()  - DATE( 1900, 1, 7 ), 7 ),
  0, TODAY() + 1, /* Sun */
  5, TODAY() + 2 + 1, /* Fri */
  6, TODAY() + 1 + 1, /* Sat */
  TODAY() + 1 /* Default (Mon/Tue/Wed/Thur) */
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_5_myRule_4_A1_Follow_Up_Date_c</name>
        <dataType>Date</dataType>
        <expression>CASE( MOD( TODAY()  - DATE( 1900, 1, 7 ), 7 ),
  0, TODAY() + 1, /* Sun */
  5, TODAY() + 2 + 1, /* Fri */
  6, TODAY() + 1 + 1, /* Sat */
  TODAY() + 1 /* Default (Mon/Tue/Wed/Thur) */
)</expression>
    </formulas>
