    <decisions>
        <description>User can create CER Case</description>
        <name>Valid_Create</name>
        <label>Valid Create</label>
        <locationX>232</locationX>
        <locationY>268</locationY>
        <defaultConnector>
            <targetReference>Cannot_Create</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Cannot Create</defaultConnectorLabel>
        <rules>
            <name>Valid_to_Create</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ValidProfileNames</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Opportunity.Probability</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>70.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_CER_Case</targetReference>
            </connector>
            <label>Valid to Create</label>
        </rules>
    </decisions>
    <decisions>
        <name>Was_Case_Created</name>
        <label>Was Case Created</label>
        <locationX>632</locationX>
        <locationY>443</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Success</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>NewCERCaseNumber</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CaseCreateSuccessful</targetReference>
            </connector>
            <label>Success</label>
        </rules>
        <rules>
            <name>Fail</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>NewCERCaseNumber</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CaseCreateFail</targetReference>
            </connector>
            <label>Fail</label>
        </rules>
    </decisions>
    <description>Final Flow</description>
    <formulas>
        <name>SubjecText</name>
        <dataType>String</dataType>
        <expression>&quot;New CER Case related to &quot;+{!Get_Opportunity.Name}</expression>
    </formulas>
    <formulas>
        <description>Valid profiles that can create CER Cases</description>
        <name>ValidProfileNames</name>
        <dataType>Boolean</dataType>
        <expression>IF(OR({!$User.ProfileName__c} = &apos;HRS Virtual Sales Rep&apos;, {!$User.ProfileName__c} = &apos;HRS Regional Sales Admin – SB&apos;, {!$User.ProfileName__c} = &apos;HRS Sales Manager – SB&apos;, {!$User.ProfileName__c} = &apos;HRS Sales Administration – SB&apos;,{!$User.ProfileName__c} = &apos;System Administrator&apos;), True, False)</expression>
    </formulas>
