/*
 *    Retrieves the available picklist values for the default record type
 *
 *    Note: This does NOT work for the Subject field on the Event object -- it always returns all values for some reason
 */

@RestResource(urlMapping='/defaultPicklistValues/*')
global class DefaultPicklistValues
{
    @HttpGet
    global static List<Picklist> getValues()
    {
        String objectType = null;
        if (RestContext.request != null)
        {
            objectType = RestContext.request.params.get('objectType');
        }
        
        if (String.isBlank(objectType))
        {
            return new List<Picklist>();
        }
    
        return getValuesHelper(objectType);
    }

    global static List<Picklist> getValuesHelper(String objectType)
    {
        try
        {
            HttpRequest req = buildRequest(objectType);
            
            Http http = new Http();
            HttpResponse resp = http.send(req);
      
            XmlStreamReader xsr = new XmlStreamReader(resp.getBody());
    
            return parsePicklistValues(xsr);
        }
        catch (Exception e)
        {
            // todo - return some error msg?
            return new List<Picklist>();
        }
    }

    global static HttpRequest buildRequest(String objectType)
    {
        String sessionId = UserInfo.getSessionId();

        String soapTemplate = '<?xml version="1.0" encoding="utf-8"?>' +
                            '<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" ' +
                              'xmlns:urn="urn:enterprise.soap.sforce.com">' +
                              '<soapenv:Header>' + 
                                 '<urn:SessionHeader>' +
                                    '<urn:sessionId>{0}</urn:sessionId>' +
                                 '</urn:SessionHeader>' +
                              '</soapenv:Header>' +
                              '<soapenv:Body>' +
                                 '<urn:describeLayout>' +
                                    '<urn:sObjectType>{1}</urn:sObjectType>' +
                                    //'<urn:recordTypeId>012i0000000E9Ad</urn:recordTypeId>' +
                                 '</urn:describeLayout>' +
                              '</soapenv:Body>' +
                            '</soapenv:Envelope>';
        String soapEnvelope = String.format(soapTemplate, new String[] {sessionId, objectType} );

        HttpRequest req = new HttpRequest();
        req.setEndpoint('https://' + System.URL.getSalesforceBaseURL().getHost() + '/services/Soap/c/27.0');
        req.setMethod('GET');
        req.setHeader('SOAPAction', 'describeLayout');
        req.setHeader('Content-Type', 'text/xml; charset=utf-8');
        req.setBody(soapEnvelope);

        return req;
    }
    
    global static List<Picklist> parsePicklistValues(XmlStreamReader reader)
    {
        Picklist[] picklists = new Picklist[0];
        while (reader.hasNext())
        {
            if (reader.getEventType() == XmlTag.START_ELEMENT && 'RecordTypeMappings' == reader.getLocalName())
            {
                Boolean defaultType = false;
                Picklist[] tempPicklists = new Picklist[0];
            
                //check if this is the default record type
                while (reader.hasNext())
                {
                    if (reader.getEventType() == XmlTag.START_ELEMENT)
                    {
                        if ('defaultRecordTypeMapping' == reader.getLocalName())
                        {
                            String defaultTypeText = parseXmlText(reader);

                            if (!String.isBlank(defaultTypeText) && Boolean.valueOf(defaultTypeText))
                            {
                                defaultType = true;
                            }
                        }
                        else if ('picklistsForRecordType' == reader.getLocalName())
                        {
                            tempPicklists.add(parsePicklistValue(reader));
                        }
                    }
                    else if (reader.getEventType() == XmlTag.END_ELEMENT && 'recordTypeMappings' == reader.getLocalName())
                    {
                        break;
                    }
                    reader.next();
                }
                
                if (defaultType)
                {
                    picklists.addAll(tempPicklists);
                    break;
                }
            }
            reader.next();
        }
        return picklists;
    }

    global static String parseXmlText(XmlStreamReader reader)
    {
        while (reader.hasNext())
        {
            if (reader.getEventType() == XmlTag.CHARACTERS)
            {
                return reader.getText();
            }
            else if (reader.getEventType() == XmlTag.END_ELEMENT)
            {
                break;
            }
            
            reader.next();
        }

        return null;
    }

    global static Picklist parsePicklistValue(XmlStreamReader reader)
    {
        Picklist picklist = new Picklist();
        while (reader.hasNext())
        {
            if (reader.getEventType() == XmlTag.START_ELEMENT)
            {
                if ('picklistName' == reader.getLocalName())
                {
                    picklist.name = parseXmlText(reader);
                }
                else if ('picklistValues' == reader.getLocalName())
                {
                    Boolean isActive = false;
                    String[] values = new String[0];
                    String[] labels = new String[0];
                    
                    while (reader.hasNext())
                    {
                        if ('active' == reader.getLocalName())
                        {
                            String isActiveText = parseXmlText(reader);

                            if (!String.isBlank(isActiveText) && Boolean.valueOf(isActiveText))
                            {
                                isActive = true;
                            }
                        }
                        else if ('label' == reader.getLocalName())
                        {
                            String label = parseXmlText(reader);

                            if (label != null)
                            {
                                labels.add(label);
                            }    
                        }
                        else if ('value' == reader.getLocalName())
                        {
                            String value = parseXmlText(reader);

                            if (value != null)
                            {
                                values.add(value);
                            }    
                        }
                        else if (reader.getEventType() == XmlTag.END_ELEMENT && 'picklistValues' == reader.getLocalName())
                        {
                            break;
                        }
                        
                        reader.next();
                    }
                    
                    if (isActive) 
                    { 
                        picklist.values.addAll(values); 
                        picklist.labels.addAll(labels); 
                    }
                }
            }
            else if (reader.getEventType() == XmlTag.END_ELEMENT && 'picklistsForRecordType' == reader.getLocalName())
            {
                break;
            }
            
            reader.next();
        }
        return picklist;
    }
    
    global class Picklist
    {
        global String name = '';
        global List<String> labels = new List<String>();
        global List<String> values = new List<String>();
    }

