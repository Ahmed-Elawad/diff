    @isTest
    private static void test_parsePicklistValue()
    {
        //valid data
        String xmlData = '<picklistsForRecordType><picklistName>GroupEventType</picklistName><picklistValues><active>true</active><defaultValue>true</defaultValue><label>Non-group Event</label><value>0</value></picklistValues><picklistValues><active>true</active><defaultValue>false</defaultValue><label>Group Event</label><value>1</value></picklistValues><picklistValues><active>true</active><defaultValue>false</defaultValue><label>Proposed Event</label><value>2</value></picklistValues></picklistsForRecordType>';
        XmlStreamReader reader = new XmlStreamReader(xmlData);

        Picklist picklist = DefaultPicklistValues.parsePicklistValue(reader);
        System.assertEquals('GroupEventType', picklist.name);
        System.assertEquals(3, picklist.values.size());
        System.assertEquals('0', picklist.values[0]);
        System.assertEquals('Non-group Event', picklist.labels[0]);
        System.assertEquals('1', picklist.values[1]);
        System.assertEquals('Group Event', picklist.labels[1]);
        System.assertEquals('2', picklist.values[2]);
        System.assertEquals('Proposed Event', picklist.labels[2]);
    }
