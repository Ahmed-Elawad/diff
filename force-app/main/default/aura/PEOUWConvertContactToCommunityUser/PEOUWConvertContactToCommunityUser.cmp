<aura:component controller="ConvertContactToCommunityUser" implements="force:lightningQuickActionWithoutHeader,lightning:isUrlAddressable,lightning:utilityItem,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="public">
    
    <aura:html tag="style">
        .cuf-content {
        padding: 0 0rem !important;
        background-color: #F2F4F7;
        }
        .slds-p-around--medium {
        padding: 0rem !important;
        }       
        .slds-modal__content{
        height:unset !important;
        max-height:unset !important; 
        background-color: #F2F4F7;
        }
        .slds-modal__container{
        width: 90% !important;
        max-width: 95% !important;
        padding-bottom:2%;
        padding-top:2%;
        } 
        .slds-modal__content:last-child {
        box-shadow: unset;
        min-height: auto; <!--80% !important-->
        }
        <!--SFDC-13614  APR0134709-Rohith-->
        .toastMessage.forceActionsText{
        white-space : pre-line !important;
        }
        <!--SFDC-13614  APR0134709-Rohith-->
    </aura:html>
    <!-- initialize by:
  check existing community suer
  check associated account
  handle errors on no load of either -->
    <aura:handler name="init" value="{!this}" action="{!c.initialize}" />
    
    <!-- running user attributes -->
    <!-- record ID = current contact record ID -->
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="accountID" type="Id" />
    <aura:attribute name="contactRec" type="Contact" />
    <aura:attribute name="runningUser" type="User" />
    <aura:attribute name='missingPermissionList' type='List' />
    <aura:attribute name='checklist' type='Map' />
    <aura:attribute name="environment" type="String" />
    
    <!-- new/old community user attr -->
    <aura:attribute name="communityNickName" type="String" />
    <aura:attribute name="communityUsername" type="String" />
    <aura:attribute name="communityUser" type="User" />
    <aura:attribute name="hasInactiveUser" type="Boolean" />
    <aura:attribute name="reactivationNeeded" type="Boolean" />
    <aura:attribute name="alias" type="String" />
    <aura:attribute name='community_Welcome_Messsage__c' type='String' />
    <aura:attribute name="checklistDocs" type="List"/>
    <!-- form functionality attributes -->
    <!-- control rendering, step, errors, etc -->
    <aura:attribute name="disableContinue" type="Boolean" default='false'/>
    <aura:attribute name='showForms' type='boolean' />
    <aura:attribute name='step' type='Integer' default='0' />
    <aura:attribute name='medicalReq' type='boolean' default='false'/>
    <aura:attribute name='lockAccess' type='boolean' default='true' />
    <aura:attribute name='benefitDateError' type='Boolean' />
    <aura:attribute name='FastPass' type='Boolean' default='false'/>
    <aura:attribute name='scrollToTop' type='boolean' />
    <aura:attribute name='toastMode' type='String' default='dismissible'/>
    <aura:handler name='change' value='{!v.scrollToTop}' action='{!c.handleScroll}' />
    <aura:handler name='FinishLoadingCommunityForms' event='c:FinishLoadingCommunityForms' action='{!c.finishLoadingCommunityForms}' />
    
    <!-- Form answers/values used for checklist/other record creation -->
    <aura:attribute name="WCFastPass" type="string" />
    <aura:attribute name="WCInTargetHazardGroup" type="string" />
    <aura:attribute name="WCPremiumUnderLimit" type="string" />
    <aura:attribute name='medicalRequestedVal' type='String' />
    <aura:attribute name='medicalUnderwritingPathVal' type='String' />
    <aura:attribute name='WCUnderwritingVal' type='String' />
    <aura:attribute name='currentlyHasPeoVal' type='String' />
    <aura:attribute name='benEffectiveDate' type='Date' />
    <aura:attribute name='medicalUwPath' type='String' default=""/>
    <aura:attribute name='selfRetainReason' type='String' />
    <aura:attribute name='existingChecklist' type='Map' />
    <aura:attribute name='platformVal' type='String' />
    <aura:attribute name='currentAffiliationVal' type='String' />
    <aura:attribute name='clientNumberVal' type='String' />
    <aura:attribute name='paychexAgencyClientVal' type='String' />
    <aura:attribute name='parentCompanyLegalNameVal' type='String' />
    <aura:attribute name='parentCompanyPaychexVal' type='String' />
    <aura:attribute name='parentCurrentlyHaveApprovalVal' type='String' />
    <aura:attribute name='isMedicalRequestedVal' type='String' />
    <aura:attribute name='fullUnderwritingPathVal' type='String' />
    <aura:attribute name='WhyareyouchoosingVal' type='String' />
    <aura:attribute name='isDisabled' type='boolean' />
    <!--aura:attribute name='ClientAddOn' type='boolean' /--> 
    <aura:attribute name="opportunity" type="Opportunity" />
    <aura:attribute name="underpathoptions" type="List"/>
    
    <!-- used for progress ring/loading -->
    <aura:attribute name='waitingForResp' type='boolean' />
    <aura:attribute name='progressRate' type='Integer' default='0' />
    
    <!-- client add on attributes -->
    <aura:attribute name='isClientAddON' type='Boolean' />
    <lightning:workspaceAPI aura:id="navWorkspace"/>
    <aura:attribute name='processAsClientAddOn' type='String' />
    <aura:attribute name="addOnOptions" type="List" default="[
                                                             {'label': 'Yes, continue as a client add on', 'value': 'Yes'},
                                                             {'label': 'No, Do not continue as a client add on', 'value': 'No'}
                                                             ]"/>
    
    <!--Added by jidesh-->
    <aura:attribute name="options" type="List" default="[
                                                        {'label': 'Yes', 'value': 'Yes'},
                                                        {'label': 'No', 'value': 'No'}
                                                        ]"/>
    <aura:attribute name='display_Benchmark_tab__c' type='boolean' default="false"/>
    <!--Sales child accounts selection -->	
    <aura:attribute name="childAccs" type="List"/>	
    <aura:attribute name="childAccChecklists" type="List"/>
    <aura:attribute name='showInfoMessage' type='boolean' default="false"/>
    <aura:attribute name='unCheckedProspectNames' type='String' default=""/>
    <aura:attribute name="childAccsOldValue" type="String" default=""/>	
    <aura:attribute name='noChildAccount' type='boolean' default="false"/>	
    <aura:attribute name="childAccountSelection" type="String"/>	
    <aura:attribute name="childAccountId" type="String" default=""/>	
    <aura:attribute name="message" type="String" default=""/>	
    <aura:attribute name="existingChildAccChecklists" type="String" default=""/>	
    <aura:attribute name='unCheckChildAcc' type='boolean' default="false"/>
    
    <aura:attribute name="createCommUser" type="String" default=""/>
    <!--<aura:attribute name="welcomeEmailCheck" type="String" default=""/>-->
    <aura:attribute name="welcomeEmailDate" type="DateTime" />
    <aura:attribute name="createUser" type="String" default="Create User"/>
    <aura:attribute name='fieldsMissing' type='boolean' />
    <lightning:navigation aura:id="navService"/>
    <aura:attribute name="pageReference" type="Object"/>
    
    <aura:attribute name="medicalUWPathType" type="String" default=""/>	
    <aura:attribute name="workersCompUWPathType" type="String" default=""/>
    <aura:attribute name='readOnly' type='Boolean' default='false'/>
    <aura:attribute name='fullUnderwriting' type='Boolean' default='false'/>
    <aura:attribute name='QQSPATesting' type='Boolean' default='false'/> 
    <aura:attribute name='disableSPAQQFields' type='Boolean' default='false'/> 
    <!-- Added By Harish -->
    <aura:attribute name="reasonNotInviting" type="List" default="[
                                                                  {'label': 'Broker Partnership', 'value': 'Broker Partnership'},
                                                                  {'label': 'Strategic Account', 'value': 'Strategic Account'},
                                                                  {'label': 'Current Paychex Client', 'value': 'Current Paychex Client'},
                                                                  {'label': 'Other', 'value': 'Other'}
                                                                  ]"/>
    <aura:attribute name="reasonNotInvitingPro" type="String" default=""/>
    <aura:attribute name="pleaseExplain" type="String" default=""/>
    <!--Attribute created to determine if a user has SPA QQ testing access-->
    <aura:attribute name='fullUWPathEditAccess' type='Boolean' default='false'/>
    
    <!--Attributes and events to close Quick Action Modal -->
    <aura:attribute name='closeModal' type='Boolean' default='false'/>
    <aura:handler name='change' value='{!v.closeModal}' action='{!c.closeAction}' />
    <aura:attribute name='showMsg' type='Boolean' default='false'/>
    <aura:attribute name="qqBanner" type="String" default=""/>
    <aura:attribute name='showInfo' type='Boolean' default='false'/>
    <aura:attribute name='portalCreationError' type='Boolean' default='false'/>
    
    <aura:if isTrue="{!$Label.c.PEOUWEdgemaintenance == 'true'}">
        <c:PEOUW_EdgeMaintanence/>
    </aura:if>
    <aura:if isTrue='{!v.lockAccess}'>
        <aura:set attribute='else'>
            <aura:if isTrue="{!v.portalCreationError}">
                <lightning:card>
                    <div style="padding: 0px 20px 20px 20px;">
                        <div role="dialog" tabindex="-1" aria-labelledby="modal-heading-id-1" aria-modal="true" aria-describedby="modal-content-id-1"
                             class="slds-modal slds-fade-in-open">
                            <div class="slds-modal__container">
                                <header class="slds-modal__header">
                                    <h2 id="modal-heading-id-1" style="font-size: large; color: red;">
                                        Portal Creation Error - Please Read and Verify the Listed Items
                                    </h2>
                                </header>
                                <div class="fontSize slds-p-around_medium" id="modal-content-id-1">
                                    <div>1. Are you the PEO Account Owner of the Prospect Client? If not, submit a Crossover.</div>
                                    <div>2. Is your PEO Opportunity created within the past 180 days? If not, create a new Opportunity or use a more recent Opportunity.</div>
                                    <div>3. Confirm your Opportunity Record Type is one of the following - PEO, Prism PEO, PEO Referral Revenue, ASO Referral Revenue.</div>
                                    <div>4. If your Prospect Client Record has a "CS CM Contract Status" of Terminated, Client, UnderContract, PendingTermination, PendingActivation, you must create a NEW Prospect Client Record.</div>
                                </div>
                                <footer class="slds-modal__footer">
                                    <lightning:button name='ConvertModal_cancelCreationButton'  variant="neutral" label="Return to Contact" onclick="{!c.closeModal}" />
                                </footer>
                            </div>
                        </div>
                        <div class="slds-backdrop slds-backdrop_open"/>
                    </div>
                </lightning:card>
                <aura:set attribute="else">
                    <div class="slds-col slds-size_1-of-12 slds-align_absolute-center">
                        <span> 
                        </span>
                    </div> 
                    <div aura:id ="formHeader" style="padding: 0px 20px 20px 20px;">
                        <header class="slds-modal__header" >
                            <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate" style="padding: 20px 20px 0px 20px;">Access PEO Getting Started Portal</h2>
                        </header>
                    </div>
                    <div class="slds-col slds-size_1-of-1" style="padding: 0px 20px 20px 40px;">
                        <aura:if isTrue='{! v.step == 0}'>
                            Loading, Please Wait
                        </aura:if>   
                        <aura:if isTrue='{! and(v.isClientAddON, v.step == 1)}'>
                            <lightning:card title="Client Add-on">
                                <ui:message title="Client Add-on" severity="warning" closable="false">
                                    The contact you are attempting to create a portal user is a child account of <a href="" onclick="{!c.openMultiIDParentTab}" class="uiOutputURL">{!v.contactRec.Account.SalesParent__r.Name}.</a>
                                    If you continue to create a portal user, the information provided will be processed as a client add on for the above prospect. All
                                    submission will also be processed as a client add-on. Client Add-Ons are not eligible for the Quick Quote review process. By 
                                    continuing as a Client Add On, the Getting Started Portal will be locked to the Full Underwriting Path.
                                </ui:message>
                                <lightning:radioGroup name="ConvertModal_isClientAddOnQuestion"
                                                      label="Would you like to continue to process as a Client add-on?"
                                                      aura:id="processAsClientAddOnSelection"
                                                      options="{! v.addOnOptions}"
                                                      value='{!v.processAsClientAddOn}' />
                            </lightning:card>
                        </aura:if>
                        <aura:if isTrue="{!v.processAsClientAddOn == 'No'}">
                            The following link will take you to the parent account of the multiID: <a href="" onclick="{!c.openMultiIDParentTab}" class="uiOutputURL">{!v.contactRec.Account.SalesParent__r.Name}.</a>
                        </aura:if>
                    </div>
                    <aura:if isTrue="{! or(and(v.isClientAddON, v.processAsClientAddOn == 'Yes'), not(v.isClientAddON) ) }">
                        <aura:if isTrue="{!and(v.step == 1,!v.communityUser)}">
                            <lightning:card title="User Creation">
                                <div aura:id ="askCreationDiv" class="slds-col slds-size_1-of-1" style="padding: 0px 20px 0px 20px;">
                                    <span>
                                        <lightning:radioGroup aura:id="askCreation" required="true"
                                                              name='askCreation'
                                                              label="Are you creating a Community Portal User?"
                                                              options="{! v.options}" 
                                                              onchange="{!c.checkCreateUser}"
                                                              value="{!v.createCommUser}"/>
                                    </span>
                                </div>
                                <aura:if isTrue="{!v.createCommUser == 'No'}">
                                    <br/>
                                    <div aura:id ="askReasonIndicate" class="slds-col slds-size_1-of-1" style="padding: 0px 20px 0px 20px;">
                                        <span>
                                            <lightning:radioGroup aura:id="askReason" required="true"
                                                                  name='askReason'
                                                                  label="Indicate a reason why you are not inviting the prospect"
                                                                  options="{! v.reasonNotInviting}" 
                                                                  onchange="{!c.checkCreateUser}"
                                                                  value="{!v.reasonNotInvitingPro}"/>
                                        </span>
                                    </div>
                                </aura:if>
                                <aura:if isTrue="{!and(v.createCommUser == 'No',v.reasonNotInvitingPro == 'Other')}">
                                    <br/>
                                    <div aura:id ="askPleaseExplain" class="slds-col slds-size_1-of-2" style="padding: 0px 20px 0px 20px;">
                                        <span>
                                            <lightning:input name="askPlease" label="Please Explain" value="{!v.pleaseExplain}"/>
                                            
                                        </span>
                                    </div>
                                </aura:if>
                            </lightning:card>
                        </aura:if>
                        <aura:if isTrue="{!and(v.step == 1,v.hasInactiveUser)}">
                            <lightning:card title="User Reactivation">
                                <div aura:id ="askReactivationDiv" class="slds-col slds-size_1-of-1" style="padding: 0px 20px 0px 20px;">
                                    <span>
                                        <lightning:radioGroup aura:id="askReactivation" required="true"
                                                              name='askReactivation'
                                                              label="An Inactive Community User was found, do you want to reactivate the user?"
                                                              options="{! v.options}"
                                                              value="{!v.reactivationNeeded}"/>
                                    </span>
                                </div>
                            </lightning:card>
                        </aura:if>
                        <aura:if isTrue="{!and(v.step == 1,v.communityUser)}">
                            
                            <lightning:card title="Existing Community User Information">
                                <div aura:id ="existingUserInfoDiv" class="slds-col slds-size_1-of-1" style="padding: 0px 20px 0px 20px;">
                                    <lightning:input name="ConvertModal_ExistingUserName"
                                                     label="Name"
                                                     disabled="true"
                                                     value="{!v.communityUser.Name}"
                                                     aura:id="existingUserName"/>
                                    <lightning:input name="ConvertModal_ExistingUserUsername"
                                                     label="Username"
                                                     disabled="true"
                                                     value="{!v.communityUser.Username}"
                                                     aura:id="existingUserUsername"/>
                                    <lightning:input name="ConvertModal_ExistingUserEmail"
                                                     label="Email"
                                                     disabled="true"
                                                     value="{!v.communityUser.Email}"
                                                     aura:id="existingUserEmail"/>                        
                                </div>
                            </lightning:card>
                        </aura:if>
                        <aura:if isTrue="{!v.step == 1}">
                            <aura:if isTrue="{!v.createCommUser == 'Yes'}">
                                <lightning:card title="New Community User Information">
                                    <div aura:id ="createCommUserFields" style="padding: 0px 20px 0px 20px;">
                                        <lightning:input name="ConvertModal_ContactName"
                                                         label="Contact Name"
                                                         disabled="true"
                                                         value="{!v.contactRec.Name}"
                                                         aura:id="contName"/>
                                        <lightning:input name="ConvertModal_ContactEmail"
                                                         label="Email"
                                                         value="{!v.contactRec.Email}"
                                                         disabled="true" />
                                        <lightning:input name="ConvertModal_ContactPhone"
                                                         label="Phone"
                                                         value="{!v.contactRec.Phone}"
                                                         disabled="true" />
                                        <lightning:input aura:id="alias"
                                                         name="ConvertModal_ContactAlias"
                                                         label="Community Alias"
                                                         value="{!v.alias}"
                                                         disabled="true" /> 
                                        <lightning:input aura:id="communityUsername"
                                                         type='email'
                                                         name="ConvertModal_ContactCommunityUsername"
                                                         value="{!v.communityUsername}"
                                                         label='Community Username'
                                                         fieldLevelHelp="User names must end in the email format of '@example.com'.This does not need to be a functioning email address, but must be unique if the contacts email address already exists as a user name within the Getting Started Portal" />
                                        
                                        <lightning:input aura:id="communityNickName"
                                                         name="ConvertModal_ContactCommunityNickName"
                                                         value="{!v.communityNickName}"
                                                         label='Portal Nickname'
                                                         fieldLevelHelp='The new users nickname in the portal. Must be unique'/>
                                        <lightning:textArea aura:id="community_Welcome_Messsage__c"
                                                            name="community_Welcome_Messsage__c"
                                                            label='Community Welcome Message'
                                                            value='{!v.community_Welcome_Messsage__c}'
                                                            fieldLevelHelp='The welcome message you enter will be sent to the user as a part of their welcome email to the portal site. If you leave this blank no message will be added.'/>
                                        <!--<lightning:radioGroup aura:id="welcomeEmail" required="true"
                                                      name='welcomeEmail'
                                                      label="Do you want to schedule the Welcome Email for a future time/date?"
                                                      options="{! v.options}" 
                                                      value="{!v.welcomeEmailCheck}"/>
                                <aura:if isTrue="{!v.welcomeEmailCheck == 'Yes'}">
                                    <p><lightning:input type="datetime" name="invitedate" required="true" value="{!v.welcomeEmailDate}" label="Enter a date/time (EST)" /></p>
                                </aura:if>-->
                                    </div>
                                </lightning:card><!--create comm user card-->
                            </aura:if><!--if creating comm user-->
                            <!--Sales child accounts selection (Added by Srujan)-->	
                            <aura:if isTrue="{!!v.noChildAccount}">	
                                <lightning:card>	
                                    <div aura:id ="makeChildAccountSelection" style="padding: 0px 20px 20px 20px;">	
                                        <lightning:radioGroup aura:id="childAccountSel" required="true"	
                                                              name='childAccountSel'	
                                                              label="Should all Child Accounts linked in your Prospect-Client Record be included in your submission?"	
                                                              options="{! v.options}" 	
                                                              value="{!v.childAccountSelection}"
                                                              onchange="{! c.handleChildAccountsSelection }"/>
                                        <aura:if isTrue="{!v.showInfoMessage}">
                                            Newly linked Child Prospects : {!v.unCheckedProspectNames}
                                            <div class="slds-p-around-small textColor">
                                                <p><b>A newly linked Child Account(s) has been detected on this Parent.
                                                    If a linked account should NOT be included, indicated "No" to the question above and select all Linked Accounts to be included.</b></p>
                                            </div>
                                        </aura:if>
                                        <aura:if isTrue="{!v.childAccountSelection == 'No'}">	
                                            <div class="slds-list_horizontal">	
                                                <lightning:checkboxGroup name="childAccountSelection" required="true"   	
                                                                         label="Select the Child Accounts to be included in your submission"    	
                                                                         options="{! v.childAccs }"    	
                                                                         value="{! v.childAccountId }"	
                                                                         onchange="{! c.handleChange }"	
                                                                         style="display:flex"/>	
                                                <lightning:helptext content="If you do not see a required Child Account listed, make sure you have it correctly 	
                                                                             linked to the Parent Account, via the Sales Parent field on the Child Record." iconName="utility:info" class="popover"/>	
                                            </div>	
                                        </aura:if>	
                                    </div>	
                                    <aura:if isTrue="{!v.unCheckChildAcc}">
                                        <div role="dialog" tabindex="-1" aria-labelledby="modal-heading-id-1" aria-modal="true" aria-describedby="modal-content-id-1"
                                             class="slds-modal slds-fade-in-open">
                                            <div class="slds-modal__container">
                                                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                                                    <p>{!v.message}</p>
                                                </div>
                                                
                                                <footer class="slds-modal__footer">
                                                    <button class="slds-button slds-button_outline-brand" onclick="{!c.cancelModal}">Cancel</button>
                                                    <button class="slds-button slds-button_brand" onclick="{!c.confirmModal}">Confirm</button>
                                                </footer>
                                            </div>
                                        </div>
                                        <div class="slds-backdrop slds-backdrop_open"/>
                                    </aura:if>	
                                </lightning:card>	
                            </aura:if>	
                            <!--End of changes by Srujan-->
                            <div aura:id ="formHeader2" style="padding: 0px 20px 20px 20px;">
                                <header class="slds-modal__header" >
                                    <h2 id="modal-heading-01" class="slds-modal__title slds-hyphenate" style="padding: 20px 20px 0px 20px;">Select your Prospect / Client Portal Customization</h2>
                                </header>
                            </div>
                            <lightning:card>
                                <!-- <div class="slds-col slds-size_1-of-2" style="padding-left: 1.8%;">
                            <legend class="slds-form-element__legend slds-form-element__label">Display Benchmark tab</legend>
                            <lightning:input type="toggle" name="display_Benchmark_tab__c" 
                                             aura:id="display_Benchmark_tab__c" label="Display Benchmark tab " 
                                             messageToggleActive="Yes" messageToggleInactive="No" 
                                             onchange="{!c.getToggleButtonValue}" 
                                             checked ="{!v.display_Benchmark_tab__c}"
                                             variant="label-hidden"	
                                             />
                        </div> -->
                                <div aura:id ="createCommUserFields2" style="padding: 0px 20px 20px 20px;">
                                    <!--aura:if isTrue="{!v.showMsg}">
                                        <div role="alert" 
                                             style="background-color:#fff;color:red;border-radius:0.25rem;font-size: 14px;width:60%;">
                                            {!v.qqBanner}
                                        </div>
                                    </aura:if-->
                                    <div class="slds-grid slds-wrap">
                                        <div class="slds-col slds-size_1-of-12">
                                            <legend class="slds-form-element__legend slds-form-element__label required">*</legend>
                                            <legend class="slds-form-element__legend slds-form-element__label">
                                                Platform </legend><lightning:helpText style="margin-top:-3px; float:left" content="If you have less than 5 enrolled employees, must select FLEX" />
                                        </div>
                                        <div class="slds-col slds-size_1-of-2"></div>
                                        <div class="slds-col slds-size_1-of-2" style="padding: 5px 0px 0px 0px;">
                                            <lightning:select class="label-hidden" aura:id='platform'
                                                              name='platform' label='Platform'
                                                              value='{!v.platformVal}' disabled="{!v.readOnly}"
                                                              required='true'>
                                                <option value='default'>Choose one...</option>
                                                <option value="Oasis">Oasis</option>
                                                <option value="Flex">Flex</option>
                                            </lightning:select>
                                        </div>
                                    </div>
                                    <br/>
                                    
                                    <div class="slds-grid slds-wrap">
                                        <div class="slds-col slds-size_1-of-2">
                                            <legend class="slds-form-element__legend slds-form-element__label required">*</legend>
                                            <legend class="slds-form-element__legend slds-form-element__label">
                                                Current Affiliation with Paychex PEO/Oasis </legend>
                                        </div>
                                        <div class="slds-col slds-size_1-of-2"></div>
                                        <div class="slds-col slds-size_1-of-2" style="padding: 5px 0px 0px 0px;">
                                            <lightning:select class="label-hidden" aura:id='currentAffiliation'
                                                              name='currentAffiliation' label='Current Affiliation with Paychex PEO/Oasis'
                                                              value='{!v.currentAffiliationVal}' disabled="{!v.readOnly}"
                                                              required='true'>
                                                <option value='default'>Choose one...</option>
                                                <option value="Paychex Payroll/ASO Client">Paychex Payroll/ASO Client</option>
                                                <option value="Paychex PEO/Oasis PEO, Child Add-On">Paychex PEO/Oasis PEO, Child Add-On</option>
                                                <option value="None">None</option>
                                            </lightning:select>
                                            
                                        </div>
                                        <div class="slds-col slds-size_1-of-2"></div>
                                        <aura:if isTrue="{!v.currentAffiliationVal=='Paychex Payroll/ASO Client'}">
                                            <div class="slds-col slds-size_1-of-2">
                                                <lightning:layout>
                                                    <lightning:layoutItem size="12">
                                                        <div style="padding: 5px 0px 0px 0px;">
                                                            <div class="slds-col slds-size_1-of-2">
