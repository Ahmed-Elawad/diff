                            <c:PEOUWcommunityForms scrollFlag='{!v.scrollToTop}' requestedMedical='{!v.medicalReq}' recordId='{!v.recordId}' WCFastPass='{!v.FastPass}'/>
                        </div>
                    </aura:if>
                </aura:set>
            </aura:if>
        </aura:set>
    </aura:if>
    <aura:if isTrue="{!v.waitingForResp}">
        <lightning:spinner aura:id="loadingSpinner" 
                           alternativeText="Updating record for !v.currRecord.Name" 
                           size="large" />
    </aura:if>
    
</aura:component>