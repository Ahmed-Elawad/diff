<aura:component description="BannerProductSensitivity">
    <aura:attribute name="sensitivityList" type="string" />
    <aura:attribute name="acctCC" type = "boolean"/> 
    <aura:attribute name = "display" type = "string" />
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

 
    <div class="box" title="Open Cases">
        <div class="banner-header slds-align_absolute-center">
            <h4 class="slds-item_label slds-text-color_weak slds-truncate slds-text-align_center">Product Sensitivity</h4>
        </div>
        <div class="banner-body">
            <div class="slds-text-color_error" title="Product Sensitivity">
              <!-- <img src="{!$Resource.BannerIcons + '/LightningIcons/yellowflag.png'}" /> -->
                <span>
                    <aura:unescapedHtml value = "{!v.display}"/>
                </span>
            </div>
        </div>
    </div> 
</aura:component>