<aura:component controller="CommunityImplementationController" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" 
                access="global" >
    
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:registerEvent name='autoSave' type='c:CommunityFormsAutoSave' />
    <aura:attribute name='saveAction' type='Object' />
    
    <!-- navigation -->
    <aura:attribute name='selectedParentTab' type='String' />
    <aura:attribute name='parentTabList' type='String[]' />
    <aura:attribute name="tabNameList" type="String[]" />
    <aura:attribute name='tabName' type='string' default="General Submission Information" />
    
    <aura:attribute name="tabStyle" type="String"/>
    <aura:attribute name='skipSave' type='Boolean' />
    <aura:attribute name='saveFunc' type='Object' />
    <aura:attribute name="isParentAccount" type="Boolean"/>
    <aura:attribute name="flexvalue" type="Boolean" />
    
    
    <!-- navigatin -->
    <aura:attribute name="implChecklists" type="Map" default="{}"/>
    <aura:attribute name="firstStep" type="Integer"/>
    <aura:attribute name="readOnlyPermission" type="Boolean" default="true" />
    <aura:attribute name='allAccounts' type='Account[]' />
    <aura:attribute name="tabType" type="String"/>
    <aura:attribute name='ParentAccountId' type='String' />
    <aura:attribute name='currentAccount' type='Account' />
    <aura:attribute name='peoChecklist' type='PEO_Onboarding_Checklist__c' />
    <aura:attribute name='implementationChecklist' type='PEO_Implementation_Checklist__c' default='{}'/>
    <aura:attribute name='parentImplChecklist' type='PEO_Implementation_Checklist__c' default='{}'/>
    <aura:attribute name='communityUser' type='Object' default="{}"/>
    <aura:attribute name='ActiveParentTab' type='String'/>
    <aura:attribute name='selectedTab' type='String' default='General Submission Information' />
    <aura:attribute name="disableQuestions" type="Boolean" default="false" />
    <aura:attribute name="disableMyStaffingPro" type="Boolean" default="false" />
    <aura:attribute name="showAdditionalFee" type="Boolean" default="false" />
    <aura:attribute name='ownerRecs' type='Object[]' />
    <aura:attribute name='ownerRecs2percent' type='Object[]' />
    <aura:attribute name='ownerRecsEarn130K' type='Object[]' />
    <aura:attribute name='ownerRecsPartnership' type='Object[]' />    
    <aura:attribute name='ownerRecsEarn150K' type='Object[]' />
    <aura:attribute name='familyMemberRecs' type='Object[]' />
    <!--JDA s08-->
    <aura:attribute name='officersRecs' type='Object[]' />
    <aura:attribute name='officersRecs185k' type='Object[]' />
    <!--JDA s08-->
    <aura:attribute name='copyfromParent' type="Boolean" />
    <aura:attribute name='mirrorParentInfo' type="Boolean" />
    <aura:attribute name='yesNoOptions' type='List' default="[{'label': 'Yes', 'value':'Yes'},
                                                             {'label':'No','value':'No'}]" />
    <aura:attribute name="selectEntityOptions" type="List" default="[{ label: '--None--', value: null },{ label: 'Single Entity', value: 'Single Entity' },
                                                                    { label: 'Controlled Only', value: 'Controlled Only' },
                                                                    { label: 'Common Only', value: 'Common Only' },
                                                                    { label: 'Part Controlled/Part Common', value: 'Part Controlled/Part Common' }]"/>
    <aura:attribute name="controlledOwnershipOptions" type="List" default="[{ label: '--None--', value: null },{ label: 'Controlled Ownership group new to PBS', value: 'Controlled Ownership group new to PBS' },
                                                                           { label: 'Existing PBS client adding a Controlled Ownership group', value: 'Existing PBS client adding a Controlled Ownership group' }]"/>
    <aura:attribute name="commonOwnershipOptions" type="List" default="[{ label: '--None--', value: null },{ label: 'Controlled Ownership group new to PBS', value: 'Controlled Ownership group new to PBS' },
                                                                       { label: 'Existing PBS client adding a Controlled Ownership group', value: 'Existing PBS client adding a Controlled Ownership group' }]"/>
    <aura:attribute name="clientCodeOptions" type="List" default="[{ label: '--None--', value: null },{ label: 'Multiple ACH Accounts', value: 'Multiple ACH Accounts' },
                                                                  { label: 'Different Period Begin/End date but same frequency', value: 'Different Period Begin/End date but same frequency' },
                                                                  { label: 'Employees in Different Client Codes', value: 'Employees in Different Client Codes' },
                                                                  { label: 'Each location is its own client code', value: 'Each location is its own client code' },
                                                                  { label: 'Separate client contacts (for confidentiality purposes)', value: 'Separate client contacts (for confidentiality purposes)' }]"/>
    <aura:attribute name="bankAccInfoOptions" type="List" default="[{ label: '--None--', value: null },{ label: 'Client will provide banking information', value: 'Client will provide banking information' },
                                                                   { label: 'Send checking account designation form', value: 'Send checking account designation form' }]"/>
    <aura:attribute name="selfRetainProducts" type="List" default="[{ label: 'Dental', value: 'Dental' },
                                                                   { label: 'Vision', value: 'Vision' },
                                                                   { label: 'MetLife Voluntary', value: 'MetLife Voluntary' }]"/>
    <!--US8 SPA-->
    <aura:attribute name="businessEntityTypeValues" type="List" 
                    default="[{ label: 'C Corporation', value: 'C Corporation' },
                             { label: 'LLC taxed as a S-Corp', value: 'LLC taxed as a S-Corp' },
                             { label: 'LLC taxed as a corporation', value: 'LLC taxed as a corporation' },
                             { label: 'LLC taxed as a partnership', value: 'LLC taxed as a partnership' },
                             { label: 'LLC taxed as a sole proprietorship', value: 'LLC taxed as a sole proprietorship' },
                             { label: 'Limited Liability Partnership', value: 'Limited Liability Partnership' },
                             { label: 'Partnership', value: 'Partnership' },
                             { label: 'Sole Proprietor', value: 'Sole Proprietor' },
                             { label: 'Non-Profit', value: 'Non-Profit' },
                             { label: 'S Corporation', value: 'S Corporation' }]"/>
    <aura:attribute name="selfRetainProductsValue" type="List"/>
    <aura:attribute name="ACIOptions" type="List" default="[{ label: 'General Ledger Setup', value: 'General Ledger Setup' },
                                                           { label: '401k Setup', value: '401k Setup' },
                                                           { label: 'Positive Pay Setup', value: 'Positive Pay Setup' }]"/>
    <aura:attribute name="frequencyOptions" type="List" default="[{ label: '52 - Weekly', value: '52 - Weekly' },
                                                                 { label: '26 - Bi-Weekly', value: '26 - Bi-Weekly' },
                                                                 { label: '24 - Semi-Monthly', value: '24 - Semi-Monthly' },
                                                                 { label: '12 - Monthly', value: '12 - Monthly' }]"/>
    <aura:attribute name="frequencyValues" type="List" default="[]"/>
    <aura:attribute name="ACIOptionsValue" type="List"/>
    <aura:attribute name="isValid" type="Boolean" default="false"/>
    <aura:attribute name="isBusinessEntityTypeScorporation" type="Boolean" default="false"/>
    <aura:attribute name="isBusinessEntityTypePartner" type="Boolean" default="false"/>
    <aura:attribute name="showWeekly" type="Boolean" default="false"/>
    <aura:attribute name="showBiWeekly" type="Boolean" default="false"/>
    <aura:attribute name="showSemiMonthly" type="Boolean" default="false"/>
    <aura:attribute name="showMonthly" type="Boolean" default="false"/>
    <aura:attribute name="isLoading" type="Boolean" default="false"/>
    <aura:handler event="c:PEOImpCopyToChildren" action="{!c.handleCopyToChildrenEvent}"/>
    <aura:attribute name='missingSectionString' type='String'/>
    <aura:attribute name='relatedFieldList' type='String[]' />
    
    <aura:attribute name="disableAddPartners" type="Boolean" default="false" />
    <aura:attribute name="scorpreadonly" type="Boolean" default="false" />
    <aura:attribute name="CCorpreadonly" type="Boolean" default="false" />
    <aura:attribute name="PartnersDelete" type="Integer" default="1"/>
    <aura:attribute name='ownerRecsPartnershipTotal' type='Integer' default='0'/>
    <aura:attribute name='businessEntityVal' type='String' default=''/>
    <!-- <aura:method name="finalizeTab" action="{!c.checkValidityController}" access="public">
    </aura:method>-->
    <!-- This attribute is used to provide delay-->
    <aura:attribute name='timeoutId' type='Integer'/>
    
    <aura:handler name="cmpEvent" event="c:PEOImpCopyParentData" action="{!c.handleCopyToChildrenEvent}"/>
    <aura:registerEvent name="appEvent" type="c:PEOImpCopyToChildren"/>
    
    <aura:if isTrue="{!v.isLoading}">
        <lightning:spinner alternativeText="Loading" size="medium" />
    </aura:if>
    
    <div class="CompInfoHeader">
        <lightning:card>
            <form class="slds-form_stacked" type='Submit' > 
                <div class=" slds-form slds-grid slds-wrap">
                    <lightning:tabset variant="vertical"  selectedTabId='{!v.tabName}' onselect="{! c.handleTabChange}">
                        <lightning:tab label="General Submission Information" id="General Submission Information"  onactive='{!c.handleTabChange}'>
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6" padding="around-small">
                                    
                                    <!--   <lightning:input type="checkbox" label="Apply information to All Children" name="input2" checked="false" onchange="{!c.handleCopyToChildren}"/> -->
                                    <!--JDA US8 SPA-->	
                                    <span style="padding: 20px;"> <label class="slds-form-element__label"> Business Entity Type</label></span>
                                    <lightning:combobox style="padding:0 18px;margin: 0px;" aura:id="Business_Entity_Type" disabled="{!v.disableQuestions}" label="Business Entity Type"  variant="label-hidden"
                                                        name="PEO_Implementation_Checklist__c.Business_Entity_Type__c" value='{!v.implementationChecklist.Business_Entity_Type__c}' 
                                                        options="{!v.businessEntityTypeValues}" onchange='{!c.handleChange}' />
                                    <aura:if isTrue="{!(v.ParentAccountId == v.currentAccount.Id)}">
                                       <!-- <lightning:input disabled='{!v.disableQuestions}' aura:id="Account.Date_of_Incorporation__c" type='Date' label="Date of Incorporation" name="Account.Date_of_Incorporation__c" 
                                                         value='{!v.currentAccount.Date_of_Incorporation__c}' onchange='{!c.handleChange}'/>-->
                                        
                                        <!--span style="padding: 20px;"> <label class="slds-form-element__label"> Select Entity Structure</label>
                                            <lightning:helptext content="Controlled Ownership Group: Single individual or entity owns 80% or more of another entity.    Brother/Sister Control Group: Two or more entities, each of which must be at least 50% owned by the same five or fewer individuals AND the combined sum of identical ownership in these companies is more than 50.   Common Ownership: Entities have some form of shared ownership. but are not legally defined as a Controlled Ownership Group" />
                                        </span>
                                        <lightning:combobox style="padding:0 18px;margin: 0px;" aura:id="Select_Entity_Structure__c" disabled="{!v.disableQuestions}" label="Select Entity Structure"  variant="label-hidden"
                                                            name="PEO_Implementation_Checklist__c.Select_Entity_Structure__c" value='{!v.implementationChecklist.Select_Entity_Structure__c}' 
                                                            options="{!v.selectEntityOptions}" onchange='{!c.handleChange}' />
                                        <aura:if isTrue="{!or(v.implementationChecklist.Select_Entity_Structure__c == 'Controlled Only',
                                                         v.implementationChecklist.Select_Entity_Structure__c == 'Part Controlled/Part Common')}">
                                            <lightning:combobox aura:id="Controlled_Ownership_Section__c" label="Controlled Ownership Section - Select One" disabled="{!v.disableQuestions}" 
                                                                name="PEO_Implementation_Checklist__c.Controlled_Ownership_Section__c" value='{!v.implementationChecklist.Controlled_Ownership_Section__c}' 
                                                                options="{!v.controlledOwnershipOptions}" onchange='{!c.handleChange}' />
                                        </aura:if>
                                        <aura:if isTrue="{!or(v.implementationChecklist.Select_Entity_Structure__c == 'Common Only',
                                                         v.implementationChecklist.Select_Entity_Structure__c == 'Part Controlled/Part Common')}">
                                            <lightning:combobox aura:id="Common_Ownership_Section__c" label="Common Ownership Section - Select One" disabled="{!v.disableQuestions}" 
                                                                name="PEO_Implementation_Checklist__c.Common_Ownership_Section__c" value='{!v.implementationChecklist.Common_Ownership_Section__c}' 
                                                                options="{!v.commonOwnershipOptions}" onchange='{!c.handleChange}' />
                                        </aura:if-->
                                    </aura:if>
                                    
                                    <aura:if isTrue="{!!(v.ParentAccountId == v.currentAccount.Id)}">
                                        <lightning:input type="checkbox" aura:id="mirrorParentInfo" label="Mirror Parent Information"  checked="{!v.mirrorParentInfo}" title="Mirror Parent Information" 
                                                         class="slds-m-right_medium" onchange="{!c.handleCopyFromParent}" disabled="{!v.disableQuestions}" fieldLevelHelp="Check here AFTER all fields are completed on the Parent Account if you wish to copy the fields automatically. This will mirror all entered information from the Implementation Questions section of Parent Account, to this Child Account only."/> 
                                        
                                    </aura:if>
                                    <lightning:input aura:id="Authorized_Officer_Name__c" label="Authorized Officer Name" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Authorized_Officer_Name__c" value='{!v.implementationChecklist.Authorized_Officer_Name__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Authorized_Officer_Title__c" label="Authorized Officer Title" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Authorized_Officer_Title__c" value='{!v.implementationChecklist.Authorized_Officer_Title__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:radioGroup aura:id="Require_multiple_client_codes__c" label="Will client require multiple client codes for this FEIN?" disabled="{!v.disableQuestions}" 
                                                          name="PEO_Implementation_Checklist__c.Require_multiple_client_codes__c" value='{!v.implementationChecklist.Require_multiple_client_codes__c}' 
                                                          options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                    <aura:if isTrue="{!v.implementationChecklist.Require_multiple_client_codes__c == 'Yes'}">
                                        <lightning:combobox aura:id="Reason_for_multiple_client_codes__c" label="Reason for multiple client codes" disabled="{!v.disableQuestions}" 
                                                            name="PEO_Implementation_Checklist__c.Reason_for_multiple_client_codes__c" value='{!v.implementationChecklist.Reason_for_multiple_client_codes__c}' 
                                                            options="{!v.clientCodeOptions}" onchange='{!c.handleChange}' />
                                    </aura:if>
                                    <aura:if isTrue="{!(v.ParentAccountId == v.currentAccount.Id)}">
                                        <lightning:input aura:id="Requested_Benefits_Effective_Date__c" type="Date" label="Requested Benefits Effective Date (1st of the month)" disabled="{!v.disableQuestions}" 
                                                         name="PEO_Implementation_Checklist__c.Requested_Benefits_Effective_Date__c" value='{!v.implementationChecklist.Requested_Benefits_Effective_Date__c}' 
                                                         onchange='{!c.handleChange}' />
                                    </aura:if>
                                </lightning:layoutItem>
                                <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6" padding="around-small">
                                     <!-- <lightning:combobox aura:id="How_bank_acc_info_be_provided__c" label="How will the bank account information be provided?" disabled="{!v.disableQuestions}" 
                                                        name="PEO_Implementation_Checklist__c.How_bank_acc_info_be_provided__c" value='{!v.implementationChecklist.How_bank_acc_info_be_provided__c}' 
                                                        options="{!v.bankAccInfoOptions}" onchange='{!c.handleChange}' /> -->
                                                        <lightning:select aura:id="How_bank_acc_info_be_provided__c" label="How will the bank account information be provided?" disabled="{!v.disableQuestions}" 
                                                        name="PEO_Implementation_Checklist__c.How_bank_acc_info_be_provided__c" value='{!v.implementationChecklist.How_bank_acc_info_be_provided__c}' onchange='{!c.handleChange}'>
                                                            
                                                        <option value="null">--None--</option>
                                                        <option value="Client will provide banking information">Client will provide banking information</option>
                                                        <option value="Send checking account designation form">Send checking account designation form</option>
                                                        <aura:if isTrue="{!v.peoChecklist.Current_Aff_with_Paychex_PEO_Oasis__c == 'Paychex Payroll/ASO Client'}">
                                                        <option value="Use Existing Bank Account" >Use Existing Bank Account</option></aura:if> 
                                                        </lightning:select>
                                    <aura:if isTrue="{!v.implementationChecklist.How_bank_acc_info_be_provided__c == 'Use Existing Bank Account'}">  
                                     <span onkeypress="{!c.keyCheck}">
                                    <lightning:input aura:id="LAST_FOUR_digits_of_their_Bank_Acc_No__c" type="text" label="Please provide the LAST FOUR digits of their Bank Account Number" disabled="{!v.disableQuestions}" 
                                                        name="PEO_Implementation_Checklist__c.LAST_FOUR_digits_of_their_Bank_Acc_No__c" value='{!v.implementationChecklist.LAST_FOUR_digits_of_their_Bank_Acc_No__c}' 
                                                        maxlength="4"/></span>
                                    </aura:if> 
                                    <aura:if isTrue="{!(v.ParentAccountId == v.currentAccount.Id)}">
                                        <lightning:radioGroup aura:id="Did_the_client_have_prior_wages__c" label="Did the client have prior wages in this calendar year?" disabled="{!v.disableQuestions}" 
                                                              name="PEO_Implementation_Checklist__c.Did_the_client_have_prior_wages__c" value='{!v.implementationChecklist.Did_the_client_have_prior_wages__c}' 
                                                              options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                        <aura:if isTrue="{!v.implementationChecklist.Did_the_client_have_prior_wages__c == 'Yes'}">
                                            <lightning:radioGroup aura:id="Year_To_Date_payments_to_be_applied__c" label="Does the client want year-to-date payments to be applied toward wage bases?" disabled="{!v.disableQuestions}" 
                                                                  name="PEO_Implementation_Checklist__c.Year_To_Date_payments_to_be_applied__c" value='{!v.implementationChecklist.Year_To_Date_payments_to_be_applied__c}' 
                                                                  options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                        </aura:if>
                                    </aura:if>
                                    <lightning:input aura:id="Same_as_Authorized_Officer__c" type="checkbox" label="Same as Authorized Officer" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Same_as_Authorized_Officer__c" checked='{!v.implementationChecklist.Same_as_Authorized_Officer__c}' 
                                                     onchange='{!c.handleCheckboxChangeAuthorized}' />
                                    <lightning:input aura:id="Business_Owner_Name__c" label="Business Owner Name" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Business_Owner_Name__c" value='{!v.implementationChecklist.Business_Owner_Name__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Business_Owner_Email_address__c" type="email" label="Business Owner Email address" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Business_Owner_Email_address__c" value='{!v.implementationChecklist.Business_Owner_Email_address__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Business_Owner_Title__c" label="Business Owner Title" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Business_Owner_Title__c" value='{!v.implementationChecklist.Business_Owner_Title__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Business_Owner_Same_as_Primary_Contact__c" type="checkbox" label="Business Owner and Primary Contact are the same" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Business_Owner_Same_as_Primary_Contact__c" checked='{!v.implementationChecklist.Business_Owner_Same_as_Primary_Contact__c}' 
                                                     onchange='{!c.handleCheckboxChange}' />
                                    <lightning:input aura:id="Primary_Contact_Name__c" label="Primary Contact Name" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Primary_Contact_Name__c" value='{!v.implementationChecklist.Primary_Contact_Name__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Primary_Contact_Email_address__c" type="email" label="Primary Contact  Email address" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Primary_Contact_Email_address__c" value='{!v.implementationChecklist.Primary_Contact_Email_address__c}' 
                                                     onchange='{!c.handleChange}' />
                                    <lightning:input aura:id="Primary_Contact_Title__c" label="Primary Contact Title" disabled="{!v.disableQuestions}" 
                                                     name="PEO_Implementation_Checklist__c.Primary_Contact_Title__c" value='{!v.implementationChecklist.Primary_Contact_Title__c}' 
                                                     onchange='{!c.handleChange}' />
                                </lightning:layoutItem>
                            </lightning:layout>
                            <!--div class="slds-text-align_right"-->
                            <div>
                                <!-- <lightning:button variant="brand" disabled="{!v.disableQuestions}" label="Save" title="Save" 
                                                  class="slds-m-right_medium" onclick="{!c.save}"/>
                                <lightning:button variant="brand" label="Next" title="Next" onclick="{!c.handleNext}"/> -->
                                <c:communityFormNavButtons tabNameList='{!v.tabNameList}' 
                                                           currentPos='{!v.tabName}' 
                                                           allPossibleParents='{!v.parentTabList}' 
                                                           activeParent='{!v.currentAccount.Id}' 
                                                           parentTabsetUsesIteration='True'
                                                           hideBackButton="{! if(and((v.ParentAccountId == v.currentAccount.Id),equals(v.tabName, 'General Submission Information')), true, false)}"
                                                           saveFunc='{!v.saveFunc}' 
                                                           skipSaveEvt='{!v.skipSave}' />
                            </div>
                            <div>
                                <abbr title="required" class="slds-required">***</abbr><p>Questions cannot be finalized until the full portal submission</p>
                            </div>
                        </lightning:tab>
                        <lightning:tab label="Setup Information" id="Setup Information"  onactive='{!c.handleTabChange}'>
                            <lightning:layout multipleRows="true">
                                <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6" padding="around-small">
                                    <aura:if isTrue="{!(v.ParentAccountId == v.currentAccount.Id)}">
                                        <!--lightning:radioGroup aura:id="Currently_offering_group_Dental__c" disabled="{!v.disableQuestions}" label="Currently offering group Dental?" 
                                                              name="PEO_Implementation_Checklist__c.Currently_offering_group_Dental__c" value='{!v.implementationChecklist.Currently_offering_group_Dental__c}' 
                                                              options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                        <lightning:radioGroup aura:id="Will_offer_Dental_through_PBS_PEO__c" disabled="{!v.disableQuestions}" label="Will offer Dental through PBS (PEO)?" 
                                                              name="PEO_Implementation_Checklist__c.Will_offer_Dental_through_PBS_PEO__c" value='{!v.implementationChecklist.Will_offer_Dental_through_PBS_PEO__c}' 
                                                              options="{!v.yesNoOptions}" onchange='{!c.handleChange}' /-->
                                        <lightning:radioGroup aura:id="Approved_to_self_retain_ancillary_prods__c" disabled="{!v.disableQuestions}" label="Is the client requesting to waive PEO Ancillary Plans?" 
                                                              name="PEO_Implementation_Checklist__c.Approved_to_self_retain_ancillary_prods__c" value='{!v.implementationChecklist.Approved_to_self_retain_ancillary_prods__c}' 
                                                              options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                        <aura:if isTrue="{!v.implementationChecklist.Approved_to_self_retain_ancillary_prods__c == 'Yes'}">
                                            <lightning:radioGroup aura:id="Requesting_to_self_retail_ancillary_prod__c" disabled="{!v.disableQuestions}" label="Is the client requesting to self retain their own ancillary benefits?" 
                                                                  name="PEO_Implementation_Checklist__c.Requesting_to_self_retail_ancillary_prod__c" value='{!v.implementationChecklist.Requesting_to_self_retail_ancillary_prod__c}' 
                                                                  options="{!v.yesNoOptions}" onchange='{!c.handleChange}' />
                                            <aura:if isTrue="{!v.implementationChecklist.Requesting_to_self_retail_ancillary_prod__c == 'Yes'}">
                                                <lightning:checkboxGroup aura:id="Self_retain_ancillary_products__c" disabled="{!v.disableQuestions}"
                                                                         name="PEO_Implementation_Checklist__c.Self_retain_ancillary_products__c" value='{!v.selfRetainProductsValue}' 
                                                                         options="{!v.selfRetainProducts}" onchange='{!c.handleChange}' />
                                                <div class="declarationText"><p>If requesting to Self Retain, please email peohbanalysts@paychex.onmicrosoft.com for approval.</p></div>
                                            </aura:if>
                                        </aura:if>
                                    </aura:if>
                                    <lightning:checkboxGroup aura:id="Payroll_Frequency__c" name="PEO_Onboarding_Checklist__c.Payroll_Frequency__c" label="Payroll Frequency (Please select all that apply)"
                                                             disabled='{!v.disableQuestions}' options="{! v.frequencyOptions }" value="{!v.frequencyValues }"
                                                             onchange='{!c.handleChange}'/>
                                    <aura:if isTrue="{!v.showWeekly}">
                                        Anticipated PEO Effective Date by Pay Frequencies: Weekly
                                        <lightning:input aura:id="Period_Begin_Dates_Weekly__c" type="Date" disabled="{!v.disableQuestions}" label="Period Begin Date" 
                                                         name="PEO_Implementation_Checklist__c.Period_Begin_Dates_Weekly__c" value='{!v.implementationChecklist.Period_Begin_Dates_Weekly__c}' 
                                                         onchange='{!c.handleChange}' />
                                        <lightning:input aura:id="Period_End_Dates_Weekly__c" type="Date" disabled="{!v.disableQuestions}" label="Period End Date" 
                                                         name="PEO_Implementation_Checklist__c.Period_End_Dates_Weekly__c" value='{!v.implementationChecklist.Period_End_Dates_Weekly__c}' 
