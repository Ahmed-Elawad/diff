<aura:component description="Powerlinks" implements="flexipage:availableForAllPageTypes" controller="UtilityBarPowerlinksAuraController">
    <aura:attribute name="listOfLinks" type="Object" />

    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <ul>
        <aura:iteration var="link" items="{!v.listOfLinks}">
            <li>
                <c:UtilityBarPowerlinksItem linkItem="{!link}" />
            </li>
        </aura:iteration>
    </ul>
</aura:component>