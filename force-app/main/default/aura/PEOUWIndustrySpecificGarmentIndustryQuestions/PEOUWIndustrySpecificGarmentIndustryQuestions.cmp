<aura:component >
    
    <!-- auto save -->
    <aura:registerEvent name='autoSave' type='c:CommunityFormsAutoSave' />
    <aura:attribute name='autoSaveRecords' type='map' />
    
    <aura:attribute name='garmentIndRec' type='Object' />
    <aura:attribute name='radioGroupOptions' type='List' default="[{'label': 'Yes', 'value':'Yes'},{'label':'No','value':'No'}]" />
    <aura:attribute name='radioOptionsSold' type='List' default="[{'label': 'Storefront', 'value':'Storefront'},{'label':'Internet','value':'Internet'},{'label':'Both','value':'Both'}]" />
    <aura:attribute name="disableEdit" type="Boolean" default="true" />
    <aura:attribute name="parentAccount" type="Account"/>
    <aura:attribute name="allAccounts" type="Account[]" />
    
    <aura:method name="validateFields" action="{!c.validate}" access="public"/>
    
 	<lightning:layout multipleRows="true">
        <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6"  padding="around-small">
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='What_items_are_manufactured__c'
                             label="What items are manufactured?"
                             onchange='{!c.handleChange}'
                             value="{!v.garmentIndRec.What_items_are_manufactured__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             type='Number'
                             fieldLevelHelp='Please enter a number.'
                             min='0'
                             name='Percentage_of_Mens_Boys_clothing__c'
                             label="Percentage of Mens &amp; Boys clothing"
                             onchange='{!c.handleChange}'
                             value="{!v.garmentIndRec.Percentage_of_Mens_Boys_clothing__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             type='Number'
                             fieldLevelHelp='Please enter a number.'
                             min='0'
                             name='Pecentage_wmn_child_inf_clothing__c'
                             onchange='{!c.handleChange}'
                             label="Percentage of Women’s, Children’s, and Infants’ clothing"
                             value="{!v.garmentIndRec.Pecentage_wmn_child_inf_clothing__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             type='Number'
                             fieldLevelHelp='Please enter a number.'
                             min='0'
                             name='Percentage_of_other_clothing__c'
                             onchange='{!c.handleChange}'
                             label="Other"
                             value="{!v.garmentIndRec.Percentage_of_other_clothing__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='What_types_and_fabric_are_used__c'
                             label="What types and fabric are used?"
                             onchange='{!c.handleChange}'
                             value="{!v.garmentIndRec.What_types_and_fabric_are_used__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             type='Number'
                             fieldLevelHelp='Please enter a number.'
                             min='0'
                             name='Percentage_of_light_weight_fabrics__c'
                             onchange='{!c.handleChange}'
                             label="Percentage of light weight fabrics such as cotton, silk, etc."
                             value="{!v.garmentIndRec.Percentage_of_light_weight_fabrics__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             type='Number'
                             fieldLevelHelp='Please enter a number.'
                             min='0'
                             name='Percentage_of_heavy_fabrics__c'
                             onchange='{!c.handleChange}'
                             label="Percentage of heavy fabrics such as vinyl, plastic, denim, leather, canvas"
                             value="{!v.garmentIndRec.Percentage_of_heavy_fabrics__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Manufactures_or_applies_fabric_applicant__c'
                                  label="Do you manufacture or apply any stain resistance free, wrinkle free or antibacterial applications?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Manufactures_or_applies_fabric_applicant__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Applications_contain_nanostructure_matts__c'
                                  label="Do applications contain nanostructure materials?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Applications_contain_nanostructure_matts__c}" />
            
             <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='nanostructures_exists_in_products__c'
                                  label="Do nanostructures exist within the applied products?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.nanostructures_exists_in_products__c}" />
            
            <aura:if isTrue="{!v.garmentIndRec.nanostructures_exists_in_products__c == 'Yes'}">
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_engineered_ventilation__c'
                                      label="Has engineered ventilation?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Has_engineered_ventilation__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_documented_hazard_assessment__c'
                                      label="Has documented hazard assessment?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Has_documented_hazard_assessment__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_written_control_plan__c'
                                      label="Has written control plan?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Has_written_control_plan__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_written_personal_protection_program__c'
                                      label="Has written personal protection program?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Has_written_personal_protection_program__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_written_hazard_communication_program__c'
                                      label="Has written hazard communication program"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Has_written_hazard_communication_program__c}" />
                <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='Other_nanostructures__c'
                                 label="Other nanostructures"
                                 onchange='{!c.handleChange}'
                                 value="{!v.garmentIndRec.Other_nanostructures__c}" />
                <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='Description_nanostructures__c'
                                 label="Description nanostructures"
                                 onchange='{!c.handleChange}'
                                 value="{!v.garmentIndRec.Description_nanostructures__c}" />
            </aura:if>
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='What_type_of_machinery_is_used__c'
                             fieldLevelHelp="Please enter 'N/A' if this does not apply"
                             label="What type of machinery is used?"
                             onchange='{!c.handleChange}'
                             value="{!v.garmentIndRec.What_type_of_machinery_is_used__c}" />
            <aura:if isTrue="{! and(v.garmentIndRec.What_type_of_machinery_is_used__c != 'N/A', v.garmentIndRec.What_type_of_machinery_is_used__c) }">
                <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='What_is_the_age_of_the_equipment__c'
                                 label="What is the age of the equipment?"
                                 onchange='{!c.handleChange}'
                                 value="{!v.garmentIndRec.What_is_the_age_of_the_equipment__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Is_the_equipment_ergonomically_adjustabl__c'
                                      label="Is the equipment ergonomically adjustable?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Is_the_equipment_ergonomically_adjustabl__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Does_equipment_have_task_lighting__c'
                                      label="Does equipment have task lighting?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Does_equipment_have_task_lighting__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Cutting_operations_done_at_this_location__c'
                                      label="Cutting operations done at this location?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Cutting_operations_done_at_this_location__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_machines_have_nip_point_guards__c'
                                      label="Do machines have nip point guards?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Do_machines_have_nip_point_guards__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Drive_mechanism_interlocks__c'
                                      label="Drive mechanism interlocks?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Drive_mechanism_interlocks__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Equipment_inspected_regularly__c'
                                      label="Equipment inspected regularly?"		
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.garmentIndRec.Equipment_inspected_regularly__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Equipment_maintenance_logged__c'
                                      label="Equipment maintenance logged?"
                                      onchange='{!c.handleChange}'
                                      options='{!v.radioGroupOptions}'
                                      value="{!v.garmentIndRec.Equipment_maintenance_logged__c}" />
            </aura:if>
        </lightning:layoutItem>
      <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6"  padding="around-small">
           
          <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Does_sample_work_or_private_label_work__c'
                                  label="Sample/Pattern Work including Embroidery-designer labels only or private label work"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Does_sample_work_or_private_label_work__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Dressmaking_or_tailoring_high_volume__c'
                                  label="Dressmaking or Tailoring-custom work exclusively, high volume, mass production."
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Dressmaking_or_tailoring_high_volume__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Cutting_sewing_contractor__c'
                                  label="Cutting/sewing contractor"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Cutting_sewing_contractor__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Silk_screening_of_fabric_or_finished_clt__c'
                                  label="Silk screening of fabric or finished clothing."
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Silk_screening_of_fabric_or_finished_clt__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Quota_or_piece_rate_method__c'
                                  label="Quota or piece rate method."
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Quota_or_piece_rate_method__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Extensive_hand_sewing__c'
                                  label="Extensive hand sewing."
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Extensive_hand_sewing__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Use_of_power_sewing_machines_electric__c'
                                  label="Use of power sewing machines &amp; electric cutters."
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Use_of_power_sewing_machines_electric__c}" />
            <lightning:radioGroup  aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Does_risk_use_any_dry_cleaning_fluids__c'
                                  label="Does risk use any dry cleaning fluids?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Does_risk_use_any_dry_cleaning_fluids__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Does_risk_use_any_special_dyes__c'
                                  label="Does risk use any special dyes?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Does_risk_use_any_special_dyes__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Pick_up_or_delivery_exposure__c'
                                  label="Pick up or delivery exposure?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Pick_up_or_delivery_exposure__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Do_any_employees_work_from_home__c'
                                  label="Do any employees work from home?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Do_any_employees_work_from_home__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Are_I_9_forms_required_on_all_employees__c'
                                  label="Are I-9 forms required on all employees?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Are_I_9_forms_required_on_all_employees__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Does_building_have_full_sprinkler_system__c'
                                  label="Does building have full sprinkler system?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Does_building_have_full_sprinkler_system__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Appropriate_of_extinguishers_1st_aid__c'
                                  label="Appropriate # of fire extinguishers/1st Aid kits?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Appropriate_of_extinguishers_1st_aid__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='Square_footage_of_work_area__c'
                             type='number'
                             min='0'
                             fieldLevelHelp='Please enter a whole number value: 0, 4, 9, etc'
                             label="Square footage of work area"
                             onchange='{!c.handleChange}'
                             options='{!v.radioGroupOptions}'
                             value="{!v.garmentIndRec.Square_footage_of_work_area__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='Number_of_emergency_exits__c'
                             type='number'
                             min='0'
                             fieldLevelHelp='Please enter a whole number value: 0, 4, 9, etc'
                             label="Number of emergency exits"
                             options='{!v.radioGroupOptions}'
                             onchange='{!c.handleChange}'
                             value="{!v.garmentIndRec.Number_of_emergency_exits__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='All_exists_accessible_and_marked__c'
                                  label="Are all exits accessible, unlocked and marked?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.All_exists_accessible_and_marked__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Emergency_evacuation_program_in_place__c'
                                  label="Emergency evacuation program in place?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Emergency_evacuation_program_in_place__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Working_area_has_adequate_lighting_venti__c'
                                  label="Working area has adequate lighting/ventilation?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.garmentIndRec.Working_area_has_adequate_lighting_venti__c}" />
        </lightning:layoutItem>
    </lightning:layout>
</aura:component>