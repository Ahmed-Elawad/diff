                                                 placeholder="Click or tap here to enter text." 
                                                 fieldLevelHelp="Please enter the personÊ¼s position at your organization"></lightning:input>
                            </span>
                        </div>
                    </aura:if>
                    <aura:if isTrue="{!v.PEOChecklist.Does_company_offer_a_retirement_plan__c == 'No'}">
                        <div class="slds-col slds-size_3-of-3" style="padding-bottom:25px">
                            <span>
                                <lightning:input disabled='{!v.readOnly}' 
                                                 label="No retirement plan explanation." 
                                                 name='no_retierment_plan_explanation__c'
                                                 value="{!v.PEOChecklist.no_retierment_plan_explanation__c}" 
                                                 onchange="{!c.handleChange}" 
                                                 placeholder="Click or tap here to enter text."></lightning:input>
                            </span>
                        </div>
                    </aura:if>
                    <aura:if isTrue="{!v.PEOChecklist.Does_company_offer_a_retirement_plan__c == 'Yes'}">
                        <div class="slds-col slds-size_3-of-3" style="padding-bottom:25px">
                            <span>
                                <lightning:input disabled='{!v.readOnly}' 
                                                 name='Who_is_your_retirement_plan_provider__c'
                                                 label="Who is your retirement plan provider?" 
                                                 value="{!v.PEOChecklist.Who_is_your_retirement_plan_provider__c}" 
                                                 onchange="{!c.handleChange}" 
                                                 placeholder="Click or tap here to enter text."></lightning:input>
                            </span>
                        </div>
                        <aura:if isTrue="{!and(v.PEOChecklist.Who_is_your_retirement_plan_provider__c != 'Paychex', v.PEOChecklist.Who_is_your_retirement_plan_provider__c != 'paychex')}">
                            <div class="slds-col slds-size_1-of-2" style="padding-bottom:25px">
                                <span>
                                    <lightning:input disabled='{!v.readOnly}' 
                                                     name='retirment_plan_kind__c'
                                                     label="Retirement Plan Type"
                                                     value="{!v.PEOChecklist.retirment_plan_kind__c}" 
                                                     onchange="{!c.handleChange}" 
                                                     placeholder="Click or tap here to enter text."></lightning:input>
                                </span>
                            </div>
                            <div class="slds-col slds-size_1-of-2" style="padding-bottom:25px">
                                <span>
                                    <lightning:input disabled='{!v.readOnly}' 
