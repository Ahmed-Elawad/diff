                                              variant="label-hidden"
                                              name='Does_company_offer_a_retirement_plan__c'
                                              label="Does your company offer a retirement plan?"
                                              options="{!v.radioGroupOptions}"
                                              onchange="{!c.clearFields}" 
                                              value="{!v.PEOChecklist.Does_company_offer_a_retirement_plan__c}"
                                              />
