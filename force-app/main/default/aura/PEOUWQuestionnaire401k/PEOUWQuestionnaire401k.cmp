                            <div class='question'>Do you have a person in your organization who manages or would manage retirement benefits?</div><br/>
                            <lightning:radioGroup  name='Manage_Retirement_Benefits__c'
                                                  variant="label-hidden"
                                                  label="Do you have a person in your organization who manages or would manage retirement benefits?"
                                                  options="{!v.radioGroupOptions}"
                                                  onchange="{!c.handleChange}" 
                                                  value="{!v.PEOChecklist.Manage_Retirement_Benefits__c}"
