                            <lightning:radioGroup disabled='{!v.readOnly}' 
                                                  variant="label-hidden"
                                                  name='Advisor_for_your_retirement_plan__c'
                                                  label="Do you work with a financial advisor for your retirement plan?"
                                                  options="{!v.radioGroupOptions}"
                                                  onchange="{!c.clearFields}" 
                                                  value="{!v.PEOChecklist.Advisor_for_your_retirement_plan__c}"
                                                  />
                            
