                            <span>
                                <div class='question'>Are you interested in working with a financial advisor?</div><br/>
                                <lightning:radioGroup disabled='{!v.readOnly}' 
                                                      variant="label-hidden"
                                                      name='Interested_working_with_financial_ad__c'
                                                      label="Are you interested in working with a financial advisor?"
                                                      options="{!v.radioGroupOptions}"
                                                      onchange="{!c.handleChange}" 
                                                      value="{!v.PEOChecklist.Interested_working_with_financial_ad__c}"
                                                      />
                                <!--lightning:input type="radio" disabled='{!v.readOnly}' label="Are you interested in working with a financial advisor?" value="{!v.PEOChecklist.Interested_working_with_financial_ad__c}" onchange="{!c.handleChange}" required="true" placeholder="Click or tap here to enter text."></lightning:input-->
                            </span>
                        </div>
                    </aura:if>
                    <div class="slds-col slds-size_3-of-3" style="padding-bottom:25px">
                        <span>
                            <div class='question'>Do you currently (or would you) provide a company contribution/match for your employees?</div><br/>
                            <lightning:radioGroup disabled='{!v.readOnly}' 
                                                  variant="label-hidden"
                                                  name='Company_contribution_for_your_employees__c'
                                                  label="Do you currently (or would you) provide a company contribution/match for your employees?"
                                                  options="{!v.radioGroupOptions}"
                                                  onchange="{!c.handleChange}" 
                                                  value="{!v.PEOChecklist.Company_contribution_for_your_employees__c}"
                                                  />
