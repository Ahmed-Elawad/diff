                                                     placeholder="Click or tap here to enter text."></lightning:input>
                                </span>
                            </div>
                           <!-- <div class="slds-col slds-size_1-of-2" style="padding-bottom:25px">
                                <span>
                                    <div class='question'>PEO plan utilized if PEO previously used</div><br/>
                                    <lightning:radioGroup name='peo_plan_utilized_if_peo_previously_used__c'
                                                          variant="label-hidden"
                                                          label="PEO plan utilized if PEO previously used"
                                                          options="{!v.radioGroupOptions}"
                                                          onchange="{!c.clearFields}" 
                                                          value="{!v.PEOChecklist.peo_plan_utilized_if_peo_previously_used__c}"
                                                          disabled='{!v.readOnly}' />
                                </span>
                            </div> -->
                            <div class="slds-col slds-size_1-of-2" style="padding-bottom:25px">
                                <span>
                                    <lightning:input disabled='{!v.readOnly}' 
                                                     name='request_for_changes_to_peo_plan_used__c'
                                                     label="Was there anything you would change about your plan?" 
                                                     value="{!v.PEOChecklist.request_for_changes_to_peo_plan_used__c}" 
                                                     onchange="{!c.handleChange}" 
                                                     placeholder="Click or tap here to enter text."></lightning:input>
                                </span>
                            </div>
                        </aura:if>
                    </aura:if>
                </div>
            </lightning:tab>
       
        
