<!-- IndustrySpecific -->
<aura:component controller="CommunityIndustrySpecificController" implements="force:appHostable,flexipage:availableForAllPageTypes" access="global" >
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    
    <!-- auto save -->
    <aura:registerEvent name='autoSave' type='c:CommunityFormsAutoSave' />
    <aura:attribute name='autoSaveRecords' type='map' />
    <aura:attribute name='saveAction' type='Object' />
    <!-- public attribute -->
    <aura:attribute name="IndustrySpecific" type="WC_Questionnaire_Industry_Specific__c"/>
    <aura:attribute name="IndustryType" type="String" />
    <aura:attribute name="IndustryTypeId" type="String" />
    <aura:attribute name="parentRecId" type="Id"/>
    <aura:attribute name="PEOChecklist" type="Object"/>
    <aura:attribute name="parentAccount" type="Account"/>
    <aura:attribute name="PolicyPeriods" type="Policy_Period__c[]"/>
    <aura:attribute name="answersChanged" type="Boolean" default="false"/>
    <aura:attribute name="policyPeriodChanged" type="Boolean" default="false"/>
    <aura:attribute name="readOnly" type="Boolean" default="true" />
    <aura:attribute name="readOnlyString" type="String" default="" />
    <aura:attribute name='radioGroupOptions' type='List' default="[{'label': 'Yes', 'value':'Yes'},{'label':'No','value':'No'}]" />
    <aura:attribute name='radioGroupOptionsApplianceSales' type='List' default="[{'label': 'Only Sell Appliances', 'value':'Only Sell Appliances'},{'label':'Only Install Appliances','value':'Only Install Appliances'},{'label':'Both Sell and Install Appliances','value':'Both Sell and Install Appliances'},{'label':'Neither sells nor installs Appliances','value':'Neither sells nor installs Appliances'}]" />
    <aura:attribute name='radioOptionsSold' type='List' default="[{'label': 'Storefront', 'value':'Storefront'},{'label':'Internet','value':'Internet'},{'label':'Both','value':'Both'}]" />
    
    <aura:attribute name="actionName" type="string" default="Next" />
    <aura:attribute name='communityUser' type='boolean' />
    <aura:attribute name="allAccounts" type="Account[]" />
    <aura:attribute name='user' type='User' />
    <aura:attribute name="loadingSpin" type="Boolean" default="true" />
    
    <!-- new attributes for parent containment of all records -->
    <aura:attribute name='industryStruct' type='Object' />
    <aura:attribute name='industryName' type='String' />
    <aura:attribute name='formSubmitted' type='boolean' default='false' />
    <aura:attribute name='buttonLabel' type='String' />
    <aura:attribute name='errMsg' type='String' />
    
    <!-- tab navigation attributes -->
    <aura:attribute name='parentTabs' type='String[]' />
    <aura:attribute name='activeParent' type='String' />
    <aura:attribute name='wcCompTabs' type='String[]' />
    <aura:attribute name='activeWcCompTab' type='String' />
    
    <!-- reference to save functionality -->
    <aura:attribute name='saveFunc' type='Object' />
    
    <!--Attributes for Naics specific questions -->
    <aura:attribute name='naicsInfo' type='String[]' />
    <aura:attribute name="displayRestaurantSpecific" type="Boolean" default="false"/>
    
    <!-- public attribute -->
    <aura:if isTrue="{!v.loadingSpin}">
        <lightning:spinner alternativeText="Loading" />
    </aura:if>
    <aura:if isTrue="{!v.IndustryType==null}">
        <h2>There no match with the NAICS code for this Account, please correct the value on Account.</h2>
        
        <aura:set attribute="else">
            <aura:if isTrue="{!greaterthan(v.readOnlyString.length, 1)}">
                <div style="width:100%">
                    <ui:message title="Evaluation Status" severity="info" closable="false" >
                        {!v.readOnlyString}
                    </ui:message>
                </div>
            </aura:if>
            <lightning:tabset aura:id="tabs" variant="scoped">
                <aura:if isTrue="{!v.IndustryType=='Doctors_Offices'}">
                    <lightning:tab label="Doctors' Office" title="Doctors_Offices" aura:id="Doctors_Offices" id="Doctors_Offices">
                        <lightning:layout>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Is_doctors_office_located_in_hospital__c'
                                                      label="Is the doctor’s office located in a hospital?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Is_doctors_office_located_in_hospital__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Do_you_provide_services_in_hospitals__c'
                                                      label="Do you go into hospitals to provide services?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Do_you_provide_services_in_hospitals__c}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.Do_you_provide_services_in_hospitals__c=='Yes'}" >
                                    <lightning:input aura:id='input'
                                                     disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                     label="If yes, how often" 
                                                     value="{!v.IndustrySpecific.if_yes_how_often__c}" />
                                </aura:if>
                                
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Are_surgeries_performed_at_the_office__c'
                                                      label="Are any surgical procedures performed at the doctor’s office?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Are_surgeries_performed_at_the_office__c}"
                                                      />
                            </lightning:layoutItem>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Are_overnight_accommodations_provided__c'
                                                      label="Are overnight accommodations provided?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Are_overnight_accommodations_provided__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Any_mobile_services_like_MRI_performed__c'
                                                      label="Are there any mobile services performed (i.e. mobile x-ray, mobile MRI, house calls, etc.)?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Any_mobile_services_like_MRI_performed__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Are_any_24_hour_services_offered__c'
                                                      label="Are any 24 hour services offered?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Are_any_24_hour_services_offered__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Do_you_provide_emergency_urgent_care__c'
                                                      label="Does your facility provide emergency/urgent care? "
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Do_you_provide_emergency_urgent_care__c}"
                                                      />
                            </lightning:layoutItem>
                            
                        </lightning:layout>
                    </lightning:tab>
                </aura:if>
                <aura:if isTrue="{!v.IndustryType=='Home_Health'}">
                    <lightning:tab label="Home Health" title="Home_Health" aura:id="Home_Health" id="Home_Health">
                        <lightning:layout>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                 label="How many patients will care givers visit in a day?" 
                                                 type='number'
                                                 value="{!v.IndustrySpecific.No_of_patients_caregivers_visit_in_24hr__c}" />
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                 label="What is the age range of the clients / patients?" 
                                                 value="{!v.IndustrySpecific.What_is_the_age_range_of_the_clients__c}" />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Will_a_manager_visit_before_sending_aid__c'
                                                      label="Will a supervisor or manager visit the client’s home before sending an aide? "
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Will_a_manager_visit_before_sending_aid__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Will_an_aide_take_the_client_to_errands__c'
                                                      label="Will the aide take the client to do errands such as Doctor’s visits, grocery shopping, beauty salon, etc.?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Will_an_aide_take_the_client_to_errands__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Any_medical_services_provided_at_home__c'
                                                      label="Any medical services provide at client’s home? "
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Any_medical_services_provided_at_home__c}"
                                                      />
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                 label="Are clients ambulatory or bed-ridden?" 
                                                 value="{!v.IndustrySpecific.Are_clients_ambulatory_or_bed_ridden__c}" />
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                 label="If both, please provide percentage of each" 
                                                 value="{!v.IndustrySpecific.If_both_please_provide_percent_of_each__c}" />
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                 label="What percentage of care givers full time (full five days / week) and part time?" 
                                                 value="{!v.IndustrySpecific.Percent_of_caregivers_fulltime_partime__c}" />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Any_system_to_monitor_caregivers_work__c'
                                                      label="Is there any sort of call-in system, GPS or tracking of the care givers to ensure they are doing only work activities and not personal errands? "
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Any_system_to_monitor_caregivers_work__c	}"
                                                      />
                            </lightning:layoutItem>
                            <lightning:layoutItem size="6"  padding="around-small">
                                
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Does_any_caregiver_provide_24hrs_care__c'
                                                      label="Do any of the care givers provide 24 hour care?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Does_any_caregiver_provide_24hrs_care__c}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.Does_any_caregiver_provide_24hrs_care__c=='Yes'}" >
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                     label="If yes, how many clients" 
                                                     type='number'
                                                     value="{!v.IndustrySpecific.if_yes_how_many_clients__c}" />
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                     label="If yes, are there multiple care givers" 
                                                     value="{!v.IndustrySpecific.if_yes_are_there_multiple_care_givers__c}" />
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                     label="If yes, what are the typical shift hours?" 
                                                     value="{!v.IndustrySpecific.if_yes_what_are_the_typical_shift_hrs__c}" />
                                </aura:if>
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Do_any_live_on_site_with_clients__c'
                                                      label="Do any live on site with clients?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Do_any_live_on_site_with_clients__c}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.Do_any_live_on_site_with_clients__c=='Yes'}" >
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                     label="If yes, how many clients" 
                                                     type='number'
                                                     value="{!v.IndustrySpecific.if_yes_how_many__c}" />
                                </aura:if>  
                                
                            </lightning:layoutItem>
                            
                        </lightning:layout>
                    </lightning:tab>
                </aura:if>
                <aura:if isTrue="{!v.IndustryType=='Hotels'}">
                    <lightning:tab label="Hotels" title="Hotels" aura:id="Hotels" id="Hotels">
                        <lightning:layout>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <aura:if isTrue="{!v.displayRestaurantSpecific== true}">
                                    <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                          aura:id='Guests_allowed_in_property__c'
                                                          label='Are guests currently allowed in the property?'
                                                          name="Guests_allowed_in_property__c"
                                                          options="{!v.radioGroupOptions}"
                                                          value="{!v.IndustrySpecific.Guests_allowed_in_property__c}"/> 
                                    <aura:if isTrue="{!v.IndustrySpecific.Guests_allowed_in_property__c == 'Yes'}">
                                        <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                              aura:id='guest_screening_upon_check_in__c'
                                                              label='Are there any types of guest screening upon check-in?'
                                                              name="guest_screening_upon_check_in__c"
                                                              options="{!v.radioGroupOptions}"
                                                              value="{!v.IndustrySpecific.guest_screening_upon_check_in__c}"/> 
                                        <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                              aura:id='additional_testing_throughout_stay__c'
                                                              label='Any additional testing done throughout the stay?'
                                                              name="additional_testing_throughout_stay__c"
                                                              options="{!v.radioGroupOptions}"
                                                              value="{!v.IndustrySpecific.additional_testing_throughout_stay__c}"/> 
                                    </aura:if>
                                    <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                          aura:id='Amenities_open_to_the_public__c'
                                                          label='Are all amenities open to the public (pool, weight room, spa, restaurant, shuttle buses, etc.)?'
                                                          name="Amenities_open_to_the_public__c"
                                                          options="{!v.radioGroupOptions}"
                                                          value="{!v.IndustrySpecific.Amenities_open_to_the_public__c}"/> 
                                    <aura:if isTrue="{!v.IndustrySpecific.Amenities_open_to_the_public__c == 'Yes'}">
                                        <lightning:textarea aura:id="Additional_cleaning_controls_public__c"
                                                            name="Additional_cleaning_controls_public__c" 
                                                            Id='Additional_cleaning_controls_public__c'
                                                            label="What additional cleaning controls have been put in place in these areas?" 
                                                            value="{!v.IndustrySpecific.Additional_cleaning_controls_public__c}"
                                                            disabled='{!v.readOnly}' onchange="{!c.handleChange}" />
                                    </aura:if>
                                    <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                          aura:id='Addtnl_ctrls_gst_clng_rooms__c'
                                                          label="Any additional controls put in place for employees cleaning guests' rooms?"
                                                          name="Addtnl_ctrls_gst_clng_rooms__c"
                                                          options="{!v.radioGroupOptions}"
                                                          value="{!v.IndustrySpecific.Addtnl_ctrls_gst_clng_rooms__c}"/> 
                                    <aura:if isTrue="{!v.IndustrySpecific.Addtnl_ctrls_gst_clng_rooms__c == 'Yes'}">
                                        <lightning:textarea aura:id="Additional_cleaning_controls_gst_rooms__c"
                                                            name="Additional_cleaning_controls_gst_rooms__c" 
                                                            Id='Additional_cleaning_controls_gst_rooms__c'
                                                            label="What additional cleaning controls have been put in place in these areas?" 
                                                            value="{!v.IndustrySpecific.Additional_cleaning_controls_gst_rooms__c}"
                                                            disabled='{!v.readOnly}' onchange="{!c.handleChange}" />
                                    </aura:if>
                                    <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                          aura:id='New_ctrls_for_deliveries_to_properties__c'
                                                          label="Any new controls in place for deliveries made to the property?"
                                                          name="New_ctrls_for_deliveries_to_properties__c"
                                                          options="{!v.radioGroupOptions}"
                                                          value="{!v.IndustrySpecific.New_ctrls_for_deliveries_to_properties__c}"/> 
                                    <aura:if isTrue="{!v.IndustrySpecific.New_ctrls_for_deliveries_to_properties__c == 'Yes'}">
                                        <lightning:textarea aura:id="Addtnl_ctrls_in_place_delivery__c"
                                                            name="Addtnl_ctrls_in_place_delivery__c" 
                                                            Id='Addtnl_ctrls_in_place_delivery__c'
                                                            label="What additional controls have been put in place in these areas?" 
                                                            value="{!v.IndustrySpecific.Addtnl_ctrls_in_place_delivery__c}"
                                                            disabled='{!v.readOnly}' onchange="{!c.handleChange}" />
                                    </aura:if>
                                </aura:if> 
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Any_restaurant_bar_on_the_premises__c'
                                                      label="Is there a restaurant or bar on the premises?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Any_restaurant_bar_on_the_premises__c}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.Any_restaurant_bar_on_the_premises__c=='Yes'}" >
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="If yes, does the entity operate this restaurant or does someone else?" value="{!v.IndustrySpecific.if_yes_does_your_entity_operate_it__c}" />
                                </aura:if>
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Does_the_hotel_offer_water_activities__c'
                                                      label="Does the hotel offer water activities (i.e. boat rentals, skiing, charter fishing, etc.)?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Does_the_hotel_offer_water_activities__c}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.Does_the_hotel_offer_water_activities__c=='Yes'}" >
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="If yes, please describe activities" value="{!v.IndustrySpecific.if_yes_please_describe_the_activities__c}" />
                                </aura:if>   
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="Who is responsible for the landscape/maintenance work at the hotel?" value="{!v.IndustrySpecific.Who_is_responsible_for_maintenance_work__c}" /> 
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Any_golf_course_owned_operated_by_hotel__c'
                                                      label="Is there a golf course that is owned and operated by the hotel?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Any_golf_course_owned_operated_by_hotel__c}"
                                                      />
                            </lightning:layoutItem>
                            
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='any_concessionaires_operating_the_hotel__c	'
                                                      label="Are there any concessionaires or independent contractors operating on the premises?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.any_concessionaires_operating_the_hotel__c	}"
                                                      />
                                <aura:if isTrue="{!v.IndustrySpecific.any_concessionaires_operating_the_hotel__c=='Yes'}" >
                                    <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="If yes, What Kind?" value="{!v.IndustrySpecific.if_yes_what_kind__c}" />
                                </aura:if>
                                
                                
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Any_laundry_facility_in_the_premises__c'
                                                      label="Do you have a laundry facility on the premises?"
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Any_laundry_facility_in_the_premises__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Do_you_provide_transportation_for_guests__c'
                                                      label="Do you provide transportation for guests going to and from airports, restaurants, etc..? "
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Do_you_provide_transportation_for_guests__c}"
                                                      />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Are_valet_services_provided__c'
                                                      label="Are valet services provided?  "
                                                      options="{!v.radioGroupOptions}"
                                                      
                                                      value="{!v.IndustrySpecific.Are_valet_services_provided__c}"
                                                      />
                                
                                
                                
                            </lightning:layoutItem>
                            
                        </lightning:layout>
                        
                    </lightning:tab>
                </aura:if>
                <aura:if isTrue="{!v.IndustryType=='Manufacturing'}">
                    <lightning:tab label="Manufacturing" title="Manufacturing" aura:id="Manufacturing" id="Manufacturing">
                        <lightning:layout>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                 label="What type of products do you mfg?" 
                                                 value="{!v.IndustrySpecific.What_are_the_type_of_products_you_mfg__c}" />
                                <ui:inputTextArea disabled='{!v.readOnly}' 
                                                  label="What is the process you follow to mfg?" 
                                                  value="{!v.IndustrySpecific.What_processes_do_you_follow_to_mfg__c}" />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                      name='Are_all_machines_properly_guarded__c'
                                                      label="Are all machines properly guarded? "
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Are_all_machines_properly_guarded__c}" />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                      name='Is_there_storage_warehousing_for_goods__c'
                                                      label="Do you provide any storage / warehousing of goods?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Is_there_storage_warehousing_for_goods__c}" />
                            </lightning:layoutItem>
                            <lightning:layoutItem size="6"  padding="around-small">
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                      name='Do_you_use_forklifts__c'
                                                      label="Do you use forklifts? "
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Do_you_use_forklifts__c}" />
                                <aura:if isTrue="{!v.IndustrySpecific.Do_you_use_forklifts__c=='Yes'}">
                                    <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                          name='if_yes_are_employees_trained_certified__c'
                                                          label="If yes, are employees trained and certified?"
                                                          options="{!v.radioGroupOptions}"
                                                          value="{!v.IndustrySpecific.if_yes_are_employees_trained_certified__c}" />
                                </aura:if>
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" 
                                                      name='Any_installation_of_parts_manufactured__c	'
                                                      label="Any installation of parts manufactured?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Any_installation_of_parts_manufactured__c	}" />
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}"
                                                      name='Any_delivery_of_goods_by_employees__c'
                                                      label="Any delivery of goods by employees?"
                                                      options="{!v.radioGroupOptions}"
                                                      value="{!v.IndustrySpecific.Any_delivery_of_goods_by_employees__c}" />
                            </lightning:layoutItem>
                        </lightning:layout>
                    </lightning:tab>
                </aura:if>
                <aura:if isTrue="{!v.IndustryType=='Restaurants'}">
                    <lightning:tab label="Restaurants" title="Restaurants" aura:id="Restaurants" id="Restaurants">
                        <lightning:layout>
                            <lightning:layoutItem size="6"  padding="around-small">
                                
                                
                                
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="What are the hours of operation? " value="{!v.IndustrySpecific.What_are_the_hours_of_operation__c}" />
                                <lightning:input disabled='{!v.readOnly}' onchange="{!c.handleChange}" label="What type of restaurant are you? For example: Wait staffed, fast food, bar food" value="{!v.IndustrySpecific.What_type_of_restaurant_are_you__c}" />
                                
                                <lightning:radioGroup disabled='{!v.readOnly}' onchange="{!c.handleChange}" name='Is_there_a_drive_thru__c'
