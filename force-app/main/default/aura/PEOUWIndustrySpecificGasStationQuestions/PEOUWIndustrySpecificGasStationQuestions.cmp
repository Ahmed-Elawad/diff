<aura:component >
    
    <!-- auto save -->
    <aura:registerEvent name='autoSave' type='c:CommunityFormsAutoSave' />
    <aura:attribute name='autoSaveRecords' type='map' />
    
    <aura:attribute name='gasIndRec' type='Object' />
    <aura:attribute name='radioGroupOptions' type='List' default="[{'label': 'Yes', 'value':'Yes'},{'label':'No','value':'No'}]" />
    <aura:attribute name='radioOptionsSold' type='List' default="[{'label': 'Storefront', 'value':'Storefront'},{'label':'Internet','value':'Internet'},{'label':'Both','value':'Both'}]" />
    <aura:attribute name='radioOptionsOverUnder5K' type='List' default="[{'label': 'Under 5,000 square ft', 'value':'Under 5,000 square ft'},{'label':'Over 5,000 square ft','value':'Over 5,000 square ft'}]" />
    <aura:attribute name='radioOptionsserviceType' type='List' default="[{'label': 'Self Service Only', 'value':'Self Service Only'},{'label':'Full Service Only','value':'Full Service Only'},{'label':'Both Full and Self Service','value':'Both Full and Self Service'}]" />
    <aura:attribute name='radioOptionsBathroomOptions' type='List' default="[{'label': 'Restrooms Inside Only', 'value':'Restrooms Inside Only'},{'label':'Restrooms Outside Only','value':'Restrooms Outside Only'},{'label':'No Restrooms','value':'No Restrooms'},{'label':'Both inside and Outside Restrooms ','value':'Both inside and Outside Restrooms '}]" />
    <aura:attribute name="disableEdit" type="Boolean" default="true" />
    <aura:attribute name="parentAccount" type="Account"/>
    <aura:attribute name="allAccounts" type="Account[]" /> 
    
    <aura:method name="validateFields" action="{!c.validate}" access="public"/>
    
    <lightning:layout multipleRows="true">
        <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6"  padding="around-small">
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Convenience_under_or_over_5000_ft__c'
                                  label="Is the convenience/grocery store under or over 5,000 square feet?"
                                  options='{!v.radioOptionsOverUnder5K}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Convenience_under_or_over_5000_ft__c}" />
            <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                             name='What_are_your_store_hours__c'
                             label="What are your store hours?"
                             onchange='{!c.handleChange}'
                             value="{!v.gasIndRec.What_are_your_store_hours__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Do_you_have_any_Deli_or_Restaurant_opera__c'
                                  label="Do you have any Deli or Restaurant operations?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Do_you_have_any_Deli_or_Restaurant_opera__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Do_you_sell_gasoline__c'
                                  label="Do you sell gasoline?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Do_you_sell_gasoline__c}" />
            <aura:if isTrue="{!v.gasIndRec.Do_you_sell_gasoline__c=='Yes'}">
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Gasoline_sales_are_90_or_more_of_sales__c'
                                      label="Do gasoline sales equal 90% or more of the total sales?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Gasoline_sales_are_90_or_more_of_sales__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Full_svc_self_svc_or_both__c'
                                      label="Do you have full-service, self-service or both?"
                                      options='{!v.radioOptionsserviceType}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Full_svc_self_svc_or_both__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_full_svc_or_automated_car_wash__c'
                                      label="Do you have a full service or automated car wash?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Has_full_svc_or_automated_car_wash__c}" />
                <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='Type_of_areas_gas_station_is_located__c'
                                 onchange='{!c.handleChange}'
                                 label="In what type of area(s) are the gas stations?  Close to the highway, suburban area, etc."
                                 value="{!v.gasIndRec.Type_of_areas_gas_station_is_located__c}" />
            </aura:if>
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Do_you_offer_any_auto_repair__c'
                                  label="Do you offer any auto repair?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Do_you_offer_any_auto_repair__c}" />
            <aura:if isTrue="{!v.gasIndRec.Do_you_offer_any_auto_repair__c=='Yes'}">
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Offers_towing_or_automobile_repair__c'
                                      label="Do you offer towing or automobile repair services?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Offers_towing_or_automobile_repair__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_you_offer_roadside_assistance_service__c'
                                      label="Do you offer roadside assistance/service?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Do_you_offer_roadside_assistance_service__c}" />
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_you_offer_any_parts_delivery__c'
                                      label="Do you offer any parts delivery?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Do_you_offer_any_parts_delivery__c}" />
                
                 <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_you_perform_services_on_air_condition__c'
                                      label="Do you perform services on air conditioning units?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Do_you_perform_services_on_air_condition__c}" />
                <aura:if isTrue="{!v.gasIndRec.Do_you_perform_services_on_air_condition__c=='Yes'}">
                    <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Has_necessary_certification_required__c'
                                      label="Do you have the necessary certification where required by state law?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Has_necessary_certification_required__c}" />	
                </aura:if>
                 
                <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_you_do_any_engine_rebuilding__c'
                                      label="Do you do any engine rebuilding?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Do_you_do_any_engine_rebuilding__c}" />	
                 <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Do_you_service_repair_heavy_truck_engine__c'
                                      label="Do you service or repair heavy truck engines or tires?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Do_you_service_repair_heavy_truck_engine__c}" />
                <aura:if isTrue="{!v.gasIndRec.Do_you_service_repair_heavy_truck_engine__c=='Yes'}">
                    <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                      name='Is_tire_cage_utilized__c'
                                      label="Is tire cage utilized?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Is_tire_cage_utilized__c}" />	
                </aura:if>
                <!--lightning:radioGroup disabled='{!v.disableEdit}' 
                                      name='Works_on_vehicles_involved_in_accidents_c'
                                      label="Do you ever work on vehicles that have been involved in accidents?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Works_on_vehicles_involved_in_accidents_c}" />
                
                <aura:if isTrue="{!v.gasIndRec.Works_on_vehicles_involved_in_accidents_c=='Yes'}">
                    <lightning:input disabled='{!v.disableEdit}' 
                                     name='Precautions_taken_for_working_on_vehicle__c'
                                     onchange='{!c.handleChange}'
                                     label="What precautions are taken to protect workers from exposure to blood bourne pathogens?"
                                     value="{!v.gasIndRec.Precautions_taken_for_working_on_vehicle__c}" />
                </aura:if-->
                <!--lightning:input disabled='{!v.disableEdit}' 
                                 name='How_are_fluids_and_materials_disposed__c'
                                 onchange='{!c.handleChange}'
                                 label="How are tires, automotive fluids, batteries, motor oil, and soiled uniforms or rags disposed of?"
                                 value="{!v.gasIndRec.How_are_fluids_and_materials_disposed__c}" /-->
                <!--lightning:radioGroup disabled='{!v.disableEdit}' 
                                      name='Gas_station_flammable_substances_in_shop__c'
                                      label="Are there any flammable substances stored in your repair shop?"
                                      options='{!v.radioGroupOptions}'
                                      onchange='{!c.handleChange}'
                                      value="{!v.gasIndRec.Gas_station_flammable_substances_in_shop__c}" />
                <aura:if isTrue="{!v.gasIndRec.Gas_station_flammable_substances_in_shop__c =='Yes'}">
                    <lightning:radioGroup disabled='{!v.disableEdit}' 
                                          name='Flammable_substances_stored_safe_gas__c'
                                          label="Are all flammable substances stored at a safe distance from potential ignition sources?"
                                          options='{!v.radioGroupOptions}'
                                          onchange='{!c.handleChange}'
                                          value="{!v.gasIndRec.Flammable_substances_stored_safe_gas__c}" />
                </aura:if-->	
            </aura:if>
            <!--lightning:input disabled='{!v.disableEdit}' 
                             name='Security_items_for_store_station__c'
                             onchange='{!c.handleChange}'
                             label="What security items are in place?  (video surveillance, panic button / alarm, firearms, bullet proof glass, no entry into location after a certain time, etc.) Please list all that apply."
                             value="{!v.gasIndRec.Security_items_for_store_station__c}" /-->
        </lightning:layoutItem>
  <lightning:layoutItem size="12" smallDeviceSize="12" mediumDeviceSize="12" largeDeviceSize="6"  padding="around-small">
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Any_employees_in_a_self_contained_units__c'
                                  label="Are any employees in a self-contained unit?"
                                  onchange='{!c.handleChange}'
                                  options='{!v.radioGroupOptions}'
                                  value="{!v.gasIndRec.Any_employees_in_a_self_contained_units__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Emp_receive_robbery_training__c'
                                  label="Do employees receive training in case of robbery?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Emp_receive_robbery_training__c}" />
            <aura:if isTrue="{!v.gasIndRec.Emp_receive_robbery_training__c =='Yes'}">
                <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='description_of_security_procedures__c'
                                 onchange='{!c.handleChange}'
                                 label="Please describe your current security/robbery procedures."
                                 value="{!v.gasIndRec.description_of_security_procedures__c}" />
            </aura:if>
            <!--lightning:radioGroup disabled='{!v.disableEdit}' 
                                  name='Is_there_a_drop_safe_on_the_premises__c'
                                  label="Is there a drop safe on the premises?"
                                  onchange='{!c.handleChange}'
                                  options='{!v.radioGroupOptions}'
                                  value="{!v.gasIndRec.Is_there_a_drop_safe_on_the_premises__c}" />
            <aura:if isTrue="{!v.gasIndRec.Is_there_a_drop_safe_on_the_premises__c =='Yes'}">
                <lightning:input disabled='{!v.disableEdit}' 
                                 name='How_many_deposits_are_made_into_safe__c'
                                 label="How many deposits are made into it?"
                                 type='Number'
                                 min='0'
                                 fieldLevelHelp='Please enter a number'
                                 onchange='{!c.handleChange}'
                                 value="{!v.gasIndRec.How_many_deposits_are_made_into_safe__c}" />
            </aura:if-->
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Has_one_employee_working_each_shift__c'
                                  label="Do you have more than one employee working in each shift?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Has_one_employee_working_each_shift__c}" />
            <lightning:radioGroup daura:id='isqQue' isabled='{!v.disableEdit}' 
                                  name='Allows_firearms_weapons_on_premisis__c'
                                  label="Do you allow any firearms or other weapons on premises?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Allows_firearms_weapons_on_premisis__c}" />
      
      		<lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Do_you_have_any_security_alarms_installe__c'
                                  label="Do you have any security alarms installed?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Do_you_have_any_security_alarms_installe__c}" />
      		<aura:if isTrue="{!v.gasIndRec.Do_you_have_any_security_alarms_installe__c =='Yes'}">
               			<lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Is_the_alarm_system_monitored__c'
                                  label="Is the alarm system monitored?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Is_the_alarm_system_monitored__c}" />
            </aura:if>	
              <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                    name='Do_you_have_any_panic_alarms_installed__c'
                                    label="Do you have any panic alarms installed?"
                                    options='{!v.radioGroupOptions}'
                                    onchange='{!c.handleChange}'
                                    value="{!v.gasIndRec.Do_you_have_any_panic_alarms_installed__c}" />
              
              <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                    name='Has_surveillance_systems_installed__c'
                                    label="Do you have any surveillance systems installed?"
                                    options='{!v.radioGroupOptions}'
                                    onchange='{!c.handleChange}'
                                    value="{!v.gasIndRec.Has_surveillance_systems_installed__c}" />
      
      		<lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                    name='Has_had_robberies_assaults_in_past_year__c'
                                    label="Have you had any robberies/assaults in the past year?"
                                    options='{!v.radioGroupOptions}'
                                    onchange='{!c.handleChange}'
                                    value="{!v.gasIndRec.Has_had_robberies_assaults_in_past_year__c}" />
      
      		<lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                    name='Do_police_do_random_patrols__c'
                                    label="Do police do random patrols?"
                                    options='{!v.radioGroupOptions}'
                                    onchange='{!c.handleChange}'
                                    value="{!v.gasIndRec.Do_police_do_random_patrols__c}" />
      			<aura:if isTrue="{!v.gasIndRec.Do_police_do_random_patrols__c =='Yes'}">
      				  <lightning:input aura:id='isqQue' disabled='{!v.disableEdit}' 
                                 name='How_often_do_police_patrol__c'
                                 onchange='{!c.handleChange}'
                                 label="How often?"
                                 value="{!v.gasIndRec.How_often_do_police_patrol__c}" />
      			</aura:if>
      
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Offers_refill_dispensing_of_LPB_tanks__c'
                                  label="Do you offer refill or dispensing of LPG tanks?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Offers_refill_dispensing_of_LPB_tanks__c}" />
            <lightning:radioGroup aura:id='isqQue' disabled='{!v.disableEdit}' 
                                  name='Has_ATM_on_premises__c'
                                  label="Do you have an ATM located on your premises?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Has_ATM_on_premises__c}" />
            <!--lightning:radioGroup disabled='{!v.disableEdit}' 
                                  name='Do_you_offer_check_cashing_services__c'
                                  label="Do you offer check cashing services?"
                                  options='{!v.radioGroupOptions}'
                                  onchange='{!c.handleChange}'
                                  value="{!v.gasIndRec.Do_you_offer_check_cashing_services__c}" /-->
        </lightning:layoutItem>
    </lightning:layout>
</aura:component>