<aura:component description="BannerStrategicAccount">
    <aura:attribute name="accountName" type="String" />

    <div class="box" title="Strategic Account">
        <div class="banner-header slds-align_absolute-center">
            <h4 class="slds-item_label slds-text-color_weak slds-truncate slds-text-align_center">Strategic Account</h4>
        </div>
        <div class="banner-body column">
            <img src="{!$Resource.BannerIcons + '/LightningIcons/strategicaccount1.png'}" />
            <span>{!v.accountName}</span>
        </div>
    </div>
</aura:component>