    <rules>
        <fullName>FA Referral notification</fullName>
        <actions>
            <name>send_referral_notification_when_Wholesaler_creates_a_referral</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>when wholesaler creates a referral, send the referee an email</description>
        <formula>AND( ISPICKVAL( Status__c , &apos;Referral Sent&apos;) , NOT(ISBLANK(Selling_Opportunity__c )), ISPICKVAL(Referral_Source__c , &apos;Wholesaler&apos;))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
