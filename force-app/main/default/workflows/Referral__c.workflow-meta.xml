    <rules>
        <fullName>Advance Partners External Referral Notification</fullName>
        <actions>
            <name>Advanced_External_Referral_Email</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Referral__c.Referral_Type__c</field>
            <operation>equals</operation>
            <value>External Referral</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.Send_Email__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Referral__c.Name</field>
            <operation>contains</operation>
            <value>Advance Partners</value>
        </criteriaItems>
        <description>Referral Notification to be sent to the Advance Partners External</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
