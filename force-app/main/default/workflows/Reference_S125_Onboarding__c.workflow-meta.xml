    <rules>
        <fullName>S125 - populate Original SR District</fullName>
        <actions>
            <name>S125_Org_Sales_Rep_District</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>S125 - populate Original SR District when the Sales_Rep__c field is initially populated</description>
        <formula>ISBLANK(Original_SR_District__c) &amp;&amp;
ISBLANK(PRIORVALUE(Sales_Rep__c)) &amp;&amp;
NOT(ISBLANK(Sales_Rep__c))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
