    <rules>
        <fullName>Audit Returned</fullName>
        <actions>
            <name>Audit_Returned_Email_S125</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Audit_Returned__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Automated Core PBA Open Enrollment Email</fullName>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7 AND 8 AND 9 AND 10</booleanFilter>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Branch_Client_Number__c</field>
            <operation>notContain</operation>
            <value>741-</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.S125_Product__c</field>
            <operation>excludes</operation>
            <value>Conversion</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.S125_Product__c</field>
            <operation>notEqual</operation>
            <value>Premium Only Plan</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Setup_Type__c</field>
            <operation>notEqual</operation>
            <value>Phase 1 PBA</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Open_Onboarding_Issues__c</field>
            <operation>notContain</operation>
            <value>Split</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Activation_Complete__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Client_Lost_During_Onboarding__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Setup_Complete_Date__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.Open_Enrollment_Notification__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_S125_Onboarding__c.CreatedDate</field>
            <operation>greaterOrEqual</operation>
            <value>11/14/2019</value>
        </criteriaItems>
        <description>Auto-sends the open enrollment email to Core clients on the day after the open enrollment start date
(filtered for anything with a created date after 11/14/19 to prevent emails triggering for clients who have already received one)</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Core_Open_Enrollment_Email_Sent</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>Update_Open_Enrollment_Notification1</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Open_Enrollment_Email_was_Sent_to_Client1</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Reference_S125_Onboarding__c.Enrollment_Open_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
