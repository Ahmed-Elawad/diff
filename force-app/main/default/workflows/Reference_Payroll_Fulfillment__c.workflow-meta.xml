    <rules>
        <fullName>RPS - Collect Setup Fee Reminder</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Payroll Fullfillment</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.Setup_Fee__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>RPS_Setup_Fee_Reminder</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>5</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>RPS_Setup_Fee_Reminder_10_Days</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>10</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>RPS_Setup_Fee_Reminder_15_Days</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>15</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Setup %3E30 Days</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Payroll Fullfillment</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.Date_Closed__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>Sends an email to the Supervisor and Account Representative if the set up is taking longer than 30 days</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Set_up_Time_frame_Exceeded</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>30</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X60_Day_Setup_Exceeded</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>60</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X90_Day_Setup_Time_Frame_Exceeded</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Reference_Payroll_Fulfillment__c.Contract_Received_Date__c</offsetFromField>
            <timeLength>90</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Tech Call Needed</fullName>
        <actions>
            <name>Tech_Call_Required</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.Tech_Call_Required__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_Payroll_Fulfillment__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Payroll Fullfillment</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
