    <rules>
        <fullName>CSA Case Status Update</fullName>
        <actions>
            <name>Case_Status_update_to_In_Progress</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>startsWith</operation>
            <value>Sales CSA</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>New</value>
        </criteriaItems>
        <criteriaItems>
            <field>EmailMessage.Incoming</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>CSA Case status is updated to In Progress from New when an outgoing email is sent</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
