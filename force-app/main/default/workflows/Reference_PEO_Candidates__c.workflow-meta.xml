    <rules>
        <fullName>Check Received Candidate</fullName>
        <actions>
            <name>Check_Received_Box</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 AND 2 AND 3) OR (1 AND 2 AND 4 AND 5 ) OR (6 AND 7 AND 3) OR (6 AND 7 AND 4 AND 5)</booleanFilter>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Candidate_Type__c</field>
            <operation>equals</operation>
            <value>Paper</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.NEP_Received__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Eligible_for_Benefits__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Eligible_for_Benefits__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Benefit_Enrollment_Received__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Candidate_Type__c</field>
            <operation>contains</operation>
            <value>SEI</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.SEI_Form_Received__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <description>When paper is received and updated for Reference PEO Candidates</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
