    <rules>
        <fullName>check Complete checkbox</fullName>
        <actions>
            <name>check_Complete_Box</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>((1 AND 2) OR (3 AND 4 AND 5) OR (3 AND 4 AND 6) OR (3 AND 7 AND 6) OR (3 AND 7 AND 8) OR (9 AND 10 AND 11) OR (9 AND 10 AND 6))</booleanFilter>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Candidate_Type__c</field>
            <operation>equals</operation>
            <value>Electronic</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Status_Description__c</field>
            <operation>equals</operation>
            <value>Completed Onboarding</value>
        </criteriaItems>
        <criteriaItems>
            <field>Reference_PEO_Candidates__c.Candidate_Type__c</field>
