    <rules>
        <fullName>Icon RTC-1000 free support expiring notification</fullName>
        <active>true</active>
        <formula>AND( ISPICKVAL( Icon_Support_Contract_Name__c, &apos;30-Day Gold Trial&apos;),  Product_and_Service__r.Name =&apos;RTC-1000&apos;, NOT(ISNULL(Contact.Email)), EndDate &lt;&gt; TODAY())</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Icon_RTC1000_free_support_expiring_notification</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>ServiceContract.EndDate</offsetFromField>
            <timeLength>-7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
