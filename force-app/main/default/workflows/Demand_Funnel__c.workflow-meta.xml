    <rules>
        <fullName>Populate EOL Fields</fullName>
        <actions>
            <name>Populate_Close_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Populate_EOL_Entered_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Populate_EOL_Entered_From</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>AND(
ISCHANGED( Current_Funnel_Status__c ),
((PRIORVALUE( Current_Funnel_Status__c ) != &quot;Reject&quot; &amp;&amp; Current_Funnel_Status__c = &quot;Reject&quot; ) || (PRIORVALUE( Current_Funnel_Status__c ) != &quot;Recycle&quot; &amp;&amp; Current_Funnel_Status__c = &quot;Recycle&quot; ) )
)
|| ( NOT(ISBLANK(Opportunity__c)) &amp;&amp; Opportunity__r.IsClosed &amp;&amp; ISBLANK(DFR_Closed_Date__c))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
