<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <customPermissions>
        <enabled>true</enabled>
        <name>P_C_Quote_Data_Access</name>
    </customPermissions>
    <description>This will grant non P&amp;C reps permission to see P&amp;C Quote Data object on their payroll oppty and populate the inital WC questions
- includes access to P&amp;C Quote Result field on opportunity</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>Opportunity.P_C_Quote_Result__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Do_you_have_Workers_Comp__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Estimated_Annual_Payroll_Wages__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Opportunity__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Renewal_Date__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Workers_Comp_Carrier_Name__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>PC_Quote_Data__c.Year_Business_Started__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Access to P&amp;C Quote Data - Payroll Rep</label>
    <objectPermissions>
        <allowCreate>true</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>PC_Quote_Data__c</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
    <pageAccesses>
        <apexPage>SubmitReferralQuoteData</apexPage>
        <enabled>true</enabled>
    </pageAccesses>
</PermissionSet>
