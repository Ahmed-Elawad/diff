function readCookie (name) 
{  
    var nameEQ = name + &quot;=&quot;;
    var ca = document.cookie.split(&apos;;&apos;);
    for(var i=0;i &lt; ca.length;i++) 
    {   
         var c = ca[i];
         while (c.charAt(0)==&apos; &apos;)
         {
             c = c.substring(1,c.length);
         }
         if (c.indexOf(nameEQ) == 0)
         {
             return c.substring(nameEQ.length,c.length);
         }
    }
    return null;
}
function getUser(asid) 
{                
    sforce.connection.sessionId = asid;
    var user = sforce.connection.getUserInfo().userName;
    sforce.connection.query(&quot;select Username, Right_Answers_Group__c from User where Username = &apos;&quot; + user + &quot;&apos;&quot;,
    {
        onSuccess: querySuccess,
        onFailure: function(response) 
        {
             document.getElementById(&quot;RAUserName&quot;).value = response;
             document.getElementById(&quot;RAUserGroup&quot;).value = response;
        }
    });
}
function querySuccess(result) 
{
    var records = result.getArray(&quot;records&quot;);
    document.getElementById(&quot;RAUserName&quot;).value = records[0].Username;
    document.getElementById(&quot;RAUserGroup&quot;).value = records[0].Right_Answers_Group__c;
}
