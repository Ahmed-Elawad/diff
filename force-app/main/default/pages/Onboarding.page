<!-- ASO Onboarding page.  Used by Sales to capture client onboarding information necessary to complete the onboarding process.
 * History
 * -------
 *  12/10/2016  Jermaine Stukes Created
 *  01/30/2018  Jermaine Stukes Updated VF Page to no longer reference components
 *  02/12/2018  Jermaine Stukes Added Handbook Contact
 *  04/07/2019  Jermaine Stukes Add Component
 *  04/01/2020  Matt Fritschi   Added Oracle CPQ Quote functionality
 *  05/04/2020  Brandon Vidro   Added submitConfirmation javascript for SMB NCP
 *  11/30/2020  Matt Fritschi   Added new component for Children, NCO, and MPSC Onboarding Records. Disabled original Doc Desk component.
 *  03/17/2022	Shilpa Govindarajulu	Removed SBQQ component references
 *  6/05/2024	Jermaine Stukes		Added Javascript
-->
<apex:page controller="OnboardingController" showHeader="false" standardStylesheets="true" sidebar="true" applyHtmlTag="true" applyBodyTag="false" docType="html-5.0"> 
    <head>
        <title>Paychex Onboarding</title>
        <script>
        function saveConfirmation()
        {alert('Your changes have been saved.\nYou will be redirected back to the Opportunity');}
        function saveProcessingMessage()
        {alert('Your changes are being processed');}
        function submitConfirmation()
        {alert('Your client profile has been successfully submitted to client transition. You will receive notification of the status shortly.');}
        function NcpSubmitOnClick(objSubmitBtn)
        {
            alert('Your changes are being processed');
            objSubmitBtn.disabled = true;
            objSubmitBtn.value = 'Submitting…';
            doNcpSubmit();
        }
        function SubmitOnClick(objSubmitBtn)
        {
            alert('Your changes are being processed');
            objSubmitBtn.disabled = true;
            objSubmitBtn.value = 'Submitting…';
            doSubmit();
        }
        </script> 
    </head>
    <body>
        <style type="text/css">
            .banner {
            background-color: #000000;
            height : 65px;
            padding: 0px 0px 0px 24px;
            }
            .black_font{
            font-color: #000000;
            }
            .black_background{
            background-color: #000000;
            }
            .brand_background2{
            background-color: rgb(243,233,219);
            }
            .brand_background{
            background-color: rgb(250,247,241);
            }
            .header1 {
            text-align: left;
            color: #ffffff;
            font-family: Georgia, serif;
            font-size: 16px;
            }
            .header2 {
            text-align: left;
            color: #000000;
            font-weight: bold;
            font-family: Georgia, serif;
            font-size: 20px;
            }
            .header3 {
            text-align: left;
            color: #000000;
            font-family: Georgia, serif;
            font-size: 14px;
            }
            .label1 {
            color: #000000;
            font-family: Arial, serif;
            font-size: 16px;
            }
            @media (min-device-width: 641px)
            {
            .textBox1 {
            width: 300px;
            font-color: #000000;
            font-family: Arial, serif;
            }
            .bannerHeader
            {
            color: #ffffff;
            font-family: Georgia, serif;
            font-size: 20px;
            font-weight: bold;
            }
            }
            @media (max-device-width: 640px)
            {
            .textBox1 {
            width: 250px;
            }
            .bannerHeader
            {
            color: #ffffff;
            font-family: Helvetica, sans-serif;
            font-size: 12px;
            font-weight: bold;
            }
            }
        </style>
        <apex:slds />
        <div class="slds-scope " xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            
            <div class="banner slds-grid slds-grid_vertical-align-center slds-grid_align-center">  
                <div class="bannerHeader"><apex:image url="{!$Resource.ReBrand_Paychex_Logo_White}"  alt="Paychex Inc"/></div>
                <div class="bannerHeader">Onboarding Application</div>
            </div>
            <div class="slds-p-around_small brand_background">
                
                <apex:outputPanel id="errorMessage">
                    <apex:actionRegion rendered="{!IF(hasAccess, false, true)}">
                        <apex:outputText ><font color="red" size="3px"><b>In order to permform this action, you must either own the Opportunity or be an Opportunity Team Member.</b></font>
                        </apex:outputText>
                    </apex:actionRegion>
                </apex:outputPanel>
                
                <apex:form id="onboarding" rendered="{!IF(hasAccess, true, false)}"> 
                    
                    <apex:outputPanel Id="onboardingType">
                        <apex:actionRegion rendered="{!AND(IF(typeSelected, false, true), isChildOnboardingRec == false, onboardingType == 'Doc Desk', hasPayrollProduct)}">
                            <label class="control-label">Is this MPSC or NCO?</label>
                            <apex:selectRadio value="{!onboardingRecord.Form_Type__c}" layout="pageDirection">
                                <apex:selectOption itemValue="NCO" itemLabel="NCO"/>
                                <apex:selectOption itemValue="MPSC" itemLabel="MPSC"/>
                            </apex:selectRadio>
                            
                            <apex:commandButton styleClass="slds-button--neutral" value="Continue" action="{!continueClicked}"/>
                        </apex:actionRegion>
                    </apex:outputPanel>
                    
                    <apex:outputPanel rendered="{!OR(IF(typeSelected, true, false), isChildOnboardingRec == true, onboardingType == 'HR Solutions')}" Id="onboardingInfo">
                    <div>
                        <div class="header2" title="Prospect-Client">{!onboardingOpp.Account.Name}</div>
                        <div class="header3 slds-p-bottom_small">Opportunity • {!onboardingOpp.Name}</div>
                        <!--<label class="header3 slds-p-bottom_small">{!IF(onboardingOpp.SBQQ__PrimaryQuote__c != null, 'Primary Steelbrick Quote Number • ' + onboardingOpp.SBQQ__PrimaryQuote__r.Name + '        ', '')}</label>-->
						<label class="header3 slds-p-bottom_small">{!IF(onboardingOpp.Primary_Oracle_Quote__c != null, 'Primary Oracle Quote Number • ' + onboardingOpp.Primary_Oracle_Quote__r.Name, 'Primary Quote Number • NO PRIMARY QUOTE FOUND')}</label>
                        <!--<label class="header3 slds-p-bottom_small">{!IF(AND(onboardingOpp.Primary_Oracle_Quote__c == null, onboardingOpp.SBQQ__PrimaryQuote__c == null), 'Primary Quote Number • NO PRIMARY QUOTE FOUND', '')}</label>-->
                    </div>
                    <div align="center" class="label1 slds-required">
                        <b>Required information is highlighted with an *</b>
                    </div> 
                    <apex:outputPanel id="HRSolutionsASO">
                        <apex:actionRegion rendered="{!IF(AND(onboardingType == 'HR Solutions', isChildOnboardingRec == false), 'true', 'false')}" >
                            <c:Onboarding_HR_Solutions />
                        </apex:actionRegion>
                    </apex:outputPanel>
                     <!--
                    <apex:outputPanel id="docDesk">
                        <apex:actionRegion rendered="{!IF(AND(onboardingType == 'Doc Desk', isChildOnboardingRec == false), 'false', 'false')}"  >
                            <c:Onboarding_DocDesk />
                        </apex:actionRegion>
                    </apex:outputPanel>
                    -->
                    <apex:outputPanel id="NCO">
                        <apex:actionRegion rendered="{!IF(AND(onboardingType == 'Doc Desk', isChildOnboardingRec == false, typeSelected == true, onboardingRecord.Form_Type__c = 'NCO'), 'true', 'false')}"  >
                            <c:OnboardingNCO />
                        </apex:actionRegion>
                    </apex:outputPanel>
                    <apex:outputPanel id="MPSC">
                        <apex:actionRegion rendered="{!IF(AND(onboardingType == 'Doc Desk', isChildOnboardingRec == false, typeSelected == true, onboardingRecord.Form_Type__c = 'MPSC'), 'true', 'false')}"  >
                            <c:OnboardingMPSC />
                        </apex:actionRegion>
                    </apex:outputPanel>
                    <apex:outputPanel id="childOnboardingRec">
                        <!-- If everything goes wrong Use this yellow code as backup and comment out
                            the red below
                        
                       ! <apex:actionRegion rendered="{!IF(AND(isChildOnboardingRec,isMMS), 'true', 'false')}"  >
                          !  <c:Child_Onboarding />
                      !  </apex:actionRegion>
                       ! <apex:actionRegion rendered="{!IF(AND(isChildOnboardingRec), 'true', 'false')}"  >
                       !     <c:Onboarding_HR_Solutions />
                       ! </apex:actionRegion>

                       TODO <!-- <apex:actionRegion rendered="{!IF(AND(isChildOnboardingRec), 'true', 'false')}"  >
                       TODO     <c:Child_Onboarding />

                        TODO        </apex:actionRegion> -->
                        
                        -->
                        
                        <apex:actionRegion rendered="{!IF(AND(isChildOnboardingRec,isMMS), 'true', 'false')}"  >
                            <c:Child_Onboarding />
                        </apex:actionRegion>
                        <apex:actionRegion rendered="{!IF(AND(isChildOnboardingRec), 'true', 'false')}"  >
                            <c:Onboarding_HR_Solutions />
                        </apex:actionRegion>

                    </apex:outputPanel>
                    </apex:outputPanel>
                </apex:form>
            </div>
        </div>
    </body>   
</apex:page>