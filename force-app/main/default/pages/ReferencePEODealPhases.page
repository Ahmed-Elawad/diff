    .diagramContainer{
        height: 100px;
        padding-top: 10px;
    }
    .initalsetupContainer{
        width: 94px;
        height: 90px;
        float: left;
    }
    .circle {
        float: left;
        width: 45px;
        height: 40px;
        background: #AFAFAF;
        -moz-border-radius: 25px;
        -webkit-border-radius: 25px;
        border-radius: 25px;
    }
    .container{
        width: 59px;
