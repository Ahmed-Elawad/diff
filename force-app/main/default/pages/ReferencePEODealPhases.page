        margin-top: 19px;
        text-align:left;
    }
    .chevronContainer{
        width: 65px;
        height: 90px;
        float: left;
    }
    .chevron {
      position: relative;
      text-align: center;
      padding: 12px;
      margin-bottom: 6px;
      height: 1px;
      width: 35px;
    }
    .gray.chevron:before,.gray.chevron:after {
    background-color: {!colorGray};
    }
    .green.chevron:before,.green.chevron:after {
    background-color: {!colorGreen};
    }
    .yellow.chevron:before,.yellow.chevron:after{
    background: {!colorYellow};
    }
    .red.chevron:before,.red.chevron:after{
    background: {!colorRed};
    }
    .chevron:before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      height: 80%;
      width: 90%;
      background: inherit;
      -webkit-transform: skew(25deg, 0deg);
      -moz-transform: skew(25deg, 0deg);
      -ms-transform: skew(25deg, 0deg);
      -o-transform: skew(25deg, 0deg);
      transform: skew(25deg, 0deg);
    }
    .chevron:after {
      content: '';
      position: absolute;
      top: 20px;
      left: 0px;
      height: 80%;
      width: 90%;
      background: inherit;
      -webkit-transform: skew(-25deg, -0deg);
      -moz-transform: skew(-25deg, -0deg);
      -ms-transform: skew(-25deg, -0deg);
      -o-transform: skew(-25deg, -0deg);
      transform: skew(-25deg, -0deg);
    }​​
    
    </style>


