<apex:page controller="reportManagerController">

    <script>
        var newWin=null;
        function openLookupPopup(name)
        {
            var url="/apex/ListOfReports?namefield=" + name;
            newWin=window.open(url, 'Popup','height=500,width=600,left=100,top=100,resizable=no,scrollbars=yes,toolbar=no,status=no');
            if (window.focus) 
            {
                newWin.focus();
            }
                
            return false;
        }
                      
        function closeLookupPopup()
        {
           if (null!=newWin)
           {
              newWin.close();
           }  
        }
    </script>
