<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HasNSSRole__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>Formula that evaluates to True when the users current role contains NSS.</description>
    <formula>IF(Contains(UserRole.Name,&quot;NSS&quot;),True, False)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>HasNSSRole</label>
    <securityClassification>Internal</securityClassification>
    <type>Checkbox</type>
</CustomField>
