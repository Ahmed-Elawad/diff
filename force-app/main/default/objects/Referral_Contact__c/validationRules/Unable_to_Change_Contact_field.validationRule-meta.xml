<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unable_to_Change_Contact_field</fullName>
    <active>true</active>
    <description>Prevents Users from changing value of the Contact field if it already has a value.</description>
    <errorConditionFormula>AND( 
ISCHANGED(Contact__c), 
 (NOT(ISBLANK(PRIORVALUE(Contact__c)))),
 (NOT(BEGINS($Profile.Name,&quot;System Administrator&quot;))) 
)</errorConditionFormula>
    <errorDisplayField>Contact__c</errorDisplayField>
    <errorMessage>You cannot change the value of the Contact field.</errorMessage>
</ValidationRule>
