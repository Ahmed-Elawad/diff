<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsInline__c</fullName>
    <businessStatus>Active</businessStatus>
    <formula>OR( ISPICKVAL(FileType__c , &quot;Lead List&quot;) 
   ,ISPICKVAL(FileType__c , &quot;Referral Source List&quot;) 
   ,CONTAINS(TEXT(FileType__c) , &quot;Inline&quot;)
   )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Is this processed inline (instead of being sent to the bulk API?)</inlineHelpText>
    <label>Is Inline</label>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
