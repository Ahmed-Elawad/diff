<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ProcessSeconds__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (NOT(ISNULL(ProcessDataEnd__c)), (ProcessDataEnd__c  -  ProcessDataStart__c) * 86400 , null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of seconds it takes to process</inlineHelpText>
    <label>Process Seconds</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
