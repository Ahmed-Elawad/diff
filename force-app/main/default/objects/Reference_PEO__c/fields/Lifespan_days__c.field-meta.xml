<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Lifespan_days__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>If(ISNULL( No_Start_Date__c), Null, 

( No_Start_Date__c - Contract_Received__c) - 2*( floor((No_Start_Date__c - DATE(1985, 6, 24))/7) - floor((Contract_Received__c - DATE(1985, 6, 24))/7)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Lifespan days</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
