<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NEPs_in_Medical_UW_Timeframe__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>the number of days inbetween the fields &quot;NEP Pass to Medical Underwriting &quot; and &quot;NEP Received From Medical Underwriting&quot;.</description>
    <externalId>false</externalId>
    <formula>If(OR(ISNULL(NEP_Pass_to_Medical_Underwriting__c),ISNULL(NEP_Received_from_Medical_Underwriting__c)) , NULL ,(NEP_Received_from_Medical_Underwriting__c - NEP_Pass_to_Medical_Underwriting__c) - 2*( floor((NEP_Received_from_Medical_Underwriting__c - DATE(1985, 6, 24))/7) - floor((NEP_Pass_to_Medical_Underwriting__c - DATE(1985, 6, 24))/7)))</formula>
    <label>NEPs in Medical UW Timeframe</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
