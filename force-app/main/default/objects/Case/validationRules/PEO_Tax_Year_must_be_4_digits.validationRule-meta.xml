<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PEO_Tax_Year_must_be_4_digits</fullName>
    <active>true</active>
    <description>PEO Tax Year must be 4 digits</description>
    <errorConditionFormula>AND(NOT(ISBLANK(Tax_Year__c)), LEN(Tax_Year__c)&lt; 4)</errorConditionFormula>
    <errorMessage>Tax Year must be four digits.</errorMessage>
</ValidationRule>
