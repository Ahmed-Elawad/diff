<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ESR_Request_Received_Date</fullName>
    <active>true</active>
    <description>Request Received Date is required</description>
    <errorConditionFormula>AND(
ISPICKVAL(ESR_Team__c,&apos;Dedicated Service&apos;),
ISPICKVAL(Action__c,&apos;Transferred to Amendments&apos;),
ISBLANK(ESR_Request_Received_Date__c))</errorConditionFormula>
    <errorDisplayField>ESR_Request_Received_Date__c</errorDisplayField>
    <errorMessage>Request Received Date is required</errorMessage>
</ValidationRule>
