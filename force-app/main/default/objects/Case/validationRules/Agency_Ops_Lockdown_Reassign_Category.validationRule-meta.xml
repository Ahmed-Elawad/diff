<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agency_Ops_Lockdown_Reassign_Category</fullName>
    <active>true</active>
    <description>Prevents the Agency Reassign Category field from being updated multiple times.</description>
    <errorConditionFormula>AND(
( ISCHANGED( Agency_Reassign_Category__c ) ), 
(LEN(TEXT(PRIORVALUE( Agency_Reassign_Category__c ))) &gt; 0), 
($Profile.Name  &lt;&gt;&quot;System Administrator&quot;)
)</errorConditionFormula>
    <errorDisplayField>Agency_Reassign_Category__c</errorDisplayField>
    <errorMessage>Once set, the Agency Reassign Category field can not be modified.</errorMessage>
</ValidationRule>
