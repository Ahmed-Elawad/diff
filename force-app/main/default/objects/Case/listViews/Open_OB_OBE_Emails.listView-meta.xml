<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Open_OB_OBE_Emails</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.PRIORITY</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.Service_Support_MSP_Case,Case.Service_Support_MSP_Applicant_Case</value>
    </filters>
    <filters>
        <field>CASES.ORIGIN</field>
        <operation>equals</operation>
        <value>Email-to-Case MSP Account Info Request,Email-to-Case MSP Applicant</value>
    </filters>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>New</value>
    </filters>
    <filters>
        <field>CASES.CREATED_DATE</field>
        <operation>greaterOrEqual</operation>
        <value>2/1/2021 12:00 AM</value>
    </filters>
    <label>Open Applicant Emails</label>
    <sharedTo>
        <role>MyStaffingPro_Account_Manager</role>
        <role>MyStaffingPro_Client_Service_Manager</role>
        <role>MyStaffingPro_Client_Service_Specialist</role>
    </sharedTo>
</ListView>
