<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>KAS_Cases_with_MPSC_Cases</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>PARENT.CASE_NUMBER</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.Service_Support_KAS</value>
    </filters>
    <filters>
        <field>PARENT.CASE_NUMBER</field>
        <operation>greaterThan</operation>
        <value>1</value>
    </filters>
    <filters>
        <field>CASES.CREATED_DATE_DATE_ONLY</field>
        <operation>greaterOrEqual</operation>
        <value>4/17/2018 12:00 AM</value>
    </filters>
    <label>KAS Cases with MPSC Cases</label>
    <sharedTo>
        <role>SystemAdministrator</role>
    </sharedTo>
</ListView>
