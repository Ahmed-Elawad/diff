        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.Service_Support_TAA_Case,Case.Service_Support_Icon_Case</value>
    </filters>
    <filters>
        <field>Product__c</field>
        <operation>notEqual</operation>
        <value>TLO</value>
