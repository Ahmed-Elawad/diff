        <field>CASES.CLOSED_DATE</field>
        <operation>equals</operation>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>notEqual</operation>
        <value>Time and Attendance eHub Support,Time Attendance Billing,Time Attendance Cancel</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>notEqual</operation>
