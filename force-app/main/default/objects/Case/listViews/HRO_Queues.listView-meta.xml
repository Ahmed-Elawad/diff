<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HRO_Queues</fullName>
    <booleanFilter>(1 or 3 or 4) and 2 and 5</booleanFilter>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.PRIORITY</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>CASES.RECORDTYPE</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OWNER_NAME</field>
        <operation>equals</operation>
        <value>HRO Project Queue</value>
    </filters>
    <filters>
        <field>CASES.STATUS</field>
        <operation>notEqual</operation>
        <value>Solved</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>equals</operation>
        <value>HRO Flex Email Case Queue,HRO Preview Email Case Queue,HRO Project Queue</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>equals</operation>
        <value>HRO Preview Case Queue</value>
    </filters>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>notEqual</operation>
        <value>Case.HRO_Timecard_Case</value>
    </filters>
    <label>HRO Queue Management</label>
    <sharedTo>
        <roleAndSubordinates>HROSCSupervisorI</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorII</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorIII</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorIV</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorV</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorVI</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorVII</roleAndSubordinates>
        <roleAndSubordinates>HROSCSupervisorVIII</roleAndSubordinates>
        <roleAndSubordinates>TimeAttendanceSupervisorAS</roleAndSubordinates>
        <roleAndSubordinates>TimeAttendanceSupervisorInbound</roleAndSubordinates>
        <roleAndSubordinates>TimeAttendanceSupervisorLevel2</roleAndSubordinates>
        <roleAndSubordinates>TimeAttendanceSupervisorOperations</roleAndSubordinates>
        <roleAndSubordinates>TimeAttendanceSupervisorOutbound</roleAndSubordinates>
    </sharedTo>
</ListView>
