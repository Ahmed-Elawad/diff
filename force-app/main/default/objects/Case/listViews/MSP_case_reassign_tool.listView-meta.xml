<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MSP_case_reassign_tool</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>CASES.ORIGIN</columns>
    <columns>NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.CREATED_DATE_DATE_ONLY</columns>
    <columns>CASES.LAST_UPDATE_DATE_ONLY</columns>
    <columns>CASES.CLOSED_DATE</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.Service_Support_MSP_Case,Case.Service_Support_MSP_Applicant_Case</value>
    </filters>
    <filters>
        <field>OWNER_NAME</field>
        <operation>contains</operation>
        <value>bhamr</value>
    </filters>
    <filters>
        <field>CASES.STATUS</field>
        <operation>notEqual</operation>
        <value>Solved</value>
    </filters>
    <filters>
        <field>CASES.ORIGIN</field>
        <operation>notEqual</operation>
        <value>Live Chat,Queue Call</value>
    </filters>
    <label>MSP case reassign tool</label>
    <sharedTo>
        <group>MSP_Support_Sups</group>
        <role>MyStaffingPro_Client_Service_Manager</role>
        <role>MyStaffingPro_Client_Service_Specialist</role>
    </sharedTo>
</ListView>
