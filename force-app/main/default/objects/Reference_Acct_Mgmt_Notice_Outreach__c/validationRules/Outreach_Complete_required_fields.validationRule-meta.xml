<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Outreach_Complete_required_fields</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Outreach_Complete__c)),ISBLANK(TEXT(Outreach_Result__c)))</errorConditionFormula>
    <errorMessage>Outreach Result is required if Outreach Complete has a date</errorMessage>
</ValidationRule>
