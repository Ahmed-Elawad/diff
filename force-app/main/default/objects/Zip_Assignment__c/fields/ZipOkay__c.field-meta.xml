<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ZipOkay__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>Basically does a check to see if the rep is active.</description>
    <formula>(User__c != null &amp;&amp;  User__r.IsActive) || (User__c = null &amp;&amp;  Sales_Org__c != &quot;PAY&quot; &amp;&amp; Sales_Org__c !=&quot;MMS&quot;)</formula>
    <inlineHelpText>Is this Zip Code okay for batch processing?</inlineHelpText>
    <label>Zip Okay</label>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
