<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payroll_Anniversary_Month__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>This formula returns the name of the month of the First Run/Core Start Date  /  First_Run_Date__c value. Per Case 20316553, this field should not appear on any page layouts, as it&apos;s being used for reporting.</description>
    <externalId>false</externalId>
    <formula>CASE(MONTH(First_Run_Date__c), 1, &quot;January&quot;, 2, &quot;February&quot;, 3, &quot;March&quot;,  4, &quot;April&quot;,  5, &quot;May&quot;,  6, &quot;June&quot;, 7, &quot;July&quot;, 8, &quot;August&quot;, 9, &quot;September&quot;, 10, &quot;October&quot;, 11, &quot;November&quot;, 12, &quot;December&quot;, &quot;&quot;)</formula>
    <label>Payroll Anniversary Month</label>
    <required>false</required>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
