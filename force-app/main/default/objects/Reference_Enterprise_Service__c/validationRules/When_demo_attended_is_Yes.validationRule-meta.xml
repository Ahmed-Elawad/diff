<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>When_demo_attended_is_Yes</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL( Client_Demo__c ,&quot;Yes&quot;) &amp;&amp;
 ISBLANK( Demo_Completed__c )</errorConditionFormula>
    <errorDisplayField>Client_Demo__c</errorDisplayField>
    <errorMessage>Demo Completed is required when Demo Attended is &quot;Yes&quot;</errorMessage>
</ValidationRule>
