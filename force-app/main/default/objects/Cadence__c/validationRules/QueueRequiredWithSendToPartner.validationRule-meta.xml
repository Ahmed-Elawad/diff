<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QueueRequiredWithSendToPartner</fullName>
    <active>true</active>
    <errorConditionFormula>AND(SendToPartner__c = true,  ISBLANK( SendToPartnerQueue__c ) )</errorConditionFormula>
    <errorDisplayField>SendToPartnerQueue__c</errorDisplayField>
    <errorMessage>Send To Partner Queue has to have a valid queue for Send To Partner.</errorMessage>
</ValidationRule>
