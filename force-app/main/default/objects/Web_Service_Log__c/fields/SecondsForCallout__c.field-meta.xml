<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SecondsForCallout__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (ISBLANK(ServiceEnd__c), 0, ((ServiceEnd__c - ServiceStart__c)*86400))</formula>
    <inlineHelpText>The number of seconds between the service start and service end fields</inlineHelpText>
    <label>Seconds For Callout</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
