<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>agentsync__SSN_is_9_digits</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
  NOT(ISBLANK(agentsync__SSN__c)),
  OR(
    LEN(agentsync__SSN__c) &lt;&gt; 9,
    NOT(REGEX(agentsync__SSN__c,&quot;[0-9]{9}&quot;))
  )
)</errorConditionFormula>
    <errorDisplayField>agentsync__SSN__c</errorDisplayField>
    <errorMessage>Social Security Number must be 9 digits without dashes</errorMessage>
</ValidationRule>
