<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Intent_URL__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>This field will show the URL to bring user to the ZoomInfo Intent object if the client has intent.</description>
    <externalId>false</externalId>
    <formula>IF (  Account.Intent__c = &quot;Yes&quot;, HYPERLINK((&quot;/lightning/r/&quot;+AccountId+&quot;/related/DOZISF__Zoominfo_Intent__r/view&quot;), &quot;ZoomInfo Intent&quot;)
   ,null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Intent URL</label>
    <required>false</required>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
