<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Current_Client__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>If the Prospect-Client of the Contact has a Biz Id = Current Client, if not = Prospect</description>
    <externalId>false</externalId>
    <formula>(IF (ISNULL(Account.Bis_ID__c ), &quot;Prospect&quot;,
IF (ISNULL(Account.Lost_Date__c ), &quot;Current Client&quot;,
&quot;Lost Client&quot;) ) )</formula>
    <label>Client Status</label>
    <required>false</required>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
