<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ProcessSeconds__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (NOT(ISBLANK(ProcessEnd__c )), ( ProcessEnd__c  -ProcessStart__c)*24*60*60
, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of seconds it took to process</inlineHelpText>
    <label>Process Seconds</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
