    <formula>IF(ISPICKVAL(SOX_Status__c , &quot;Pending&quot;)||ISPICKVAL(SOX_Status__c, &quot;Complete&quot;) ||ISPICKVAL(SOX_Status__c, &quot;Duplicate Carrier&quot;), 0,1)</formula>
    <label>Sox Status None Count</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
