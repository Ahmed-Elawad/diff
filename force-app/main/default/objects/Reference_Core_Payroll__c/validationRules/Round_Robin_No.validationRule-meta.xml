<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Round_Robin_No</fullName>
    <active>true</active>
    <description>MPSC Exception</description>
    <errorConditionFormula>AND(NOT(BEGINS($Profile.Name,&apos;System Admin&apos;)),ISPICKVAL(Round_Robin__c,&apos;No&apos;),OR(ISBLANK( Assigned_IS__c ),ISBLANK( Assigned_Supervisor__c )))</errorConditionFormula>
    <errorMessage>If Round Robin is No, Assigned IS and Assigned Supervisor are required</errorMessage>
</ValidationRule>
