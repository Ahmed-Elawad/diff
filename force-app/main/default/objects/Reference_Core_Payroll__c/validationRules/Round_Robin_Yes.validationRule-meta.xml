<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Round_Robin_Yes</fullName>
    <active>true</active>
    <description>MPSC Exception</description>
    <errorConditionFormula>AND(NOT(BEGINS($Profile.Name,&apos;System Admin&apos;)),ISPICKVAL(Round_Robin__c,&apos;Yes&apos;),ISBLANK( Branch__c))</errorConditionFormula>
    <errorMessage>If Round Robin is Yes, Branch is required</errorMessage>
</ValidationRule>
