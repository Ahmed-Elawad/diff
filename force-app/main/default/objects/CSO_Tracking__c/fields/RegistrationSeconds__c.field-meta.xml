    <fullName>RegistrationSeconds__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (NOT(ISBLANK(RegistrationEnd__c )), ( RegistrationEnd__c-RegistrationStart__c)*24*60*60
 , null)</formula>
