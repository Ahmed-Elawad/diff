<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TAA_On_Boarding_Test_View</fullName>
    <columns>TASK.SUBJECT</columns>
    <columns>TASK.WHO_NAME</columns>
    <columns>TASK.STATUS</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>TASK.RECORDTYPE</columns>
    <columns>TASK.DUE_DATE</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>OWNER_ROLE</field>
        <operation>contains</operation>
        <value>Time</value>
    </filters>
    <filters>
        <field>TASK.CLOSED</field>
        <operation>equals</operation>
        <value>0</value>
    </filters>
    <filters>
        <field>TASK.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Task.x200809TimeAttendanceOnboardingTaskRecordType</value>
    </filters>
    <filters>
        <field>TASK.DUE_DATE</field>
        <operation>lessOrEqual</operation>
        <value>TODAY</value>
    </filters>
    <label>TAA: On Boarding Test View</label>
    <sharedTo>
        <group>OS_Analysts</group>
        <role>SystemAdministrator</role>
        <role>TimeAttendanceAccountSpecialist</role>
    </sharedTo>
</ListView>
