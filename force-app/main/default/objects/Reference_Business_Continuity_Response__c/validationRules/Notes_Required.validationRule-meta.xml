<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Notes_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(BEGINS($Profile.Name,&apos;System Admin&apos;)),
ISBLANK(Notes__c),
INCLUDES( Summary_of_Information_Reviewed__c,&apos;Other&apos;)
)</errorConditionFormula>
    <errorMessage>Please enter additional information in the notes field because Summary of Information contains &apos;Other&apos;</errorMessage>
</ValidationRule>
