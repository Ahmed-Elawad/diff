<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SecondsToProcess__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (NOT(ISNULL(EndTime__c)), ( EndTime__c  -  StartTime__c )*86400, 0)</formula>
    <inlineHelpText>How many seconds between the start and end time</inlineHelpText>
    <label>Seconds To Process</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
