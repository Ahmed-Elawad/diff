<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Log2Seconds__c</fullName>
    <businessStatus>Active</businessStatus>
    <externalId>false</externalId>
    <formula>IF (NOT(ISNULL(Log2End__c)), ( Log2End__c - Log2Start__c )*86400, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of seconds for the Log2 process to complete</inlineHelpText>
    <label>Log2 Seconds</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
