<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Event_Client_Number</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
   ISCHANGED( Client_Nbr__c ),
    $Profile.Name &lt;&gt; &quot;System Administrator&quot;
)</errorConditionFormula>
    <errorDisplayField>Client_Nbr__c</errorDisplayField>
    <errorMessage>Client Number displayed for reference only.</errorMessage>
</ValidationRule>
