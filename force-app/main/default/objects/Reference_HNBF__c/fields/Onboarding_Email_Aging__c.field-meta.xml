<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Onboarding_Email_Aging__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>Calculates the number of days between the date received to the date the onboarding email is sent. If date the onboarding email sent is blank, null is returned.</description>
    <externalId>false</externalId>
    <formula>ABS(IF( ISBLANK(Onboarding_email_sent__c ), NULL, Onboarding_email_sent__c-Date_Received__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Onboarding Email Aging</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
