<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Who_will_receive_valuation_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
RecordType.Name = &apos;Service Onboarding 401k Conversions&apos;,
NOT(ISBLANK(Wire_Received__c)),
ISPICKVAL(Who_will_receive_Valuation__c,&apos;&apos;),
NOT(ISPICKVAL(Prior_Recordkeeper_Fund_Manager__c, &apos;ADP&apos;))
)</errorConditionFormula>
    <errorMessage>You must fill out Who will receive valuation before filling out wire Received!</errorMessage>
</ValidationRule>
