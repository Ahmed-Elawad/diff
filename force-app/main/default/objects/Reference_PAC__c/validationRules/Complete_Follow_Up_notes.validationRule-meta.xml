<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Complete_Follow_Up_notes</fullName>
    <active>true</active>
    <errorConditionFormula>OR (
 Complete_Follow_Up_notes__c &lt; 0,
Complete_Follow_Up_notes__c &gt; 10
)</errorConditionFormula>
    <errorDisplayField>Complete_Follow_Up_notes__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-10</errorMessage>
</ValidationRule>
