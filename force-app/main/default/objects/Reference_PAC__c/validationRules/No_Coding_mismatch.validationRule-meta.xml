<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Coding_mismatch</fullName>
    <active>true</active>
    <description>No Coding mismatch</description>
    <errorConditionFormula>OR (
 No_Coding_mismatch__c &lt; 0,
No_Coding_mismatch__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>No_Coding_mismatch__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-5</errorMessage>
</ValidationRule>
