<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PAC_Procedures_followed</fullName>
    <active>true</active>
    <errorConditionFormula>OR (
 PAC_Procedures_followed__c &lt; 0,
PAC_Procedures_followed__c &gt; 8
)</errorConditionFormula>
    <errorDisplayField>PAC_Procedures_followed__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-8</errorMessage>
</ValidationRule>
