<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tax_Year_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(IRS_Late_Original_E_Filing_Needed__c, &apos;Yes&apos;),
ISPICKVAL(Tax_Year__c,&apos;&apos;))</errorConditionFormula>
    <errorMessage>The field &quot;Tax Year?&quot; cannot be BLANK if the field named &quot;IRS Late Original E-Filing Needed?&quot; equals &quot;Yes&quot;.</errorMessage>
</ValidationRule>
