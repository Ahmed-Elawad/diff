<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Bumping</fullName>
    <active>true</active>
    <errorConditionFormula>OR (
 No_Bumping__c &lt; 0,
No_Bumping__c &gt; 6
)</errorConditionFormula>
    <errorDisplayField>No_Bumping__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-6</errorMessage>
</ValidationRule>
