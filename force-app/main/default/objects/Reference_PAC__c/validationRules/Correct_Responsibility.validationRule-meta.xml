<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Correct_Responsibility</fullName>
    <active>true</active>
    <errorConditionFormula>OR (
 Correct_Responsibility__c &lt; 0,
Correct_Responsibility__c &gt; 10
)</errorConditionFormula>
    <errorDisplayField>Correct_Responsibility__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-10</errorMessage>
</ValidationRule>
