<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Email_and_docs_attached_scanned</fullName>
    <active>true</active>
    <description>Email and docs attached/scanned</description>
    <errorConditionFormula>OR (
 Email_and_docs_attached_scanned__c &lt; 0,
Email_and_docs_attached_scanned__c &gt; 5
)</errorConditionFormula>
    <errorDisplayField>Email_and_docs_attached_scanned__c</errorDisplayField>
    <errorMessage>Can only enter numbers 0-5</errorMessage>
</ValidationRule>
