<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>District_Query_Key__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>A Concatenation of the District Number and Sales Org to allow for easier querying.</description>
    <externalId>false</externalId>
    <formula>IF (Org_Type__c = &quot;District&quot;, (Sales_Org__c &amp; 
    IF( BEGINS(Org_Number__c, &apos;00&apos;) ,RIGHT(Org_Number__c,LEN(Org_Number__c)-2), 
         IF (BEGINS(Org_Number__c, &apos;0&apos;) ,RIGHT(Org_Number__c,LEN(Org_Number__c)-1), 
Org_Number__c))), &quot;&quot;)</formula>
    <inlineHelpText>A Concatenation of the District Number and Sales Org to allow for easier querying.</inlineHelpText>
    <label>District Query Key</label>
    <required>false</required>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
