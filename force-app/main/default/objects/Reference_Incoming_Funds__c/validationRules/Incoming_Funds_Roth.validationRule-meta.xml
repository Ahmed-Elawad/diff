<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Incoming_Funds_Roth</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL( Contribution_Type__c ,&apos;Roth Rollover&apos;),OR(ISBLANK( Roth_Contribution_Amt__c ),ISBLANK( Roth_Earnings__c )))</errorConditionFormula>
    <errorMessage>Roth Contribution Amount and Roth Earnings are required when Contribution Type is Roth Rollover</errorMessage>
</ValidationRule>
