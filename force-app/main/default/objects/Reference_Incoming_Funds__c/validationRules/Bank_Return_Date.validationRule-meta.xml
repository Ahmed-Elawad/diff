<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Bank_Return_Date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Bank_Return_Date__c )),ISBLANK( Bank_Return_Reason__c ))</errorConditionFormula>
    <errorDisplayField>Bank_Return_Reason__c</errorDisplayField>
    <errorMessage>Bank Return Reason is required if there is a Bank Return Date</errorMessage>
</ValidationRule>
