<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Trade_Verified_cannot_be_entered</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL(Reduced_by_Forfeitures__c ,&quot;Yes&quot;)&amp;&amp; !Forfeiture_Table_Updated__c &amp;&amp; !ISBLANK(Date_Trade_Verified__c )</errorConditionFormula>
    <errorDisplayField>Forfeiture_Table_Updated__c</errorDisplayField>
    <errorMessage>&quot;Date Trade Verified&quot; cannot be entered until the &quot;Forfeiture Table Updated&quot; field is checked.</errorMessage>
</ValidationRule>
