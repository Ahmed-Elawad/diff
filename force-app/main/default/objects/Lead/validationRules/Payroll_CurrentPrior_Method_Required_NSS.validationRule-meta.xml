<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Payroll_CurrentPrior_Method_Required_NSS</fullName>
    <active>false</active>
    <description>Makes Payroll Current-Prior Method Required for NSS users</description>
    <errorConditionFormula>AND(BEGINS($Profile.Name, &apos;NSS&apos;), ISPICKVAL(Current_Prior_Method__c,&quot;&quot;))</errorConditionFormula>
    <errorDisplayField>Current_Prior_Method__c</errorDisplayField>
    <errorMessage>Payroll Current-Prior Method is required</errorMessage>
</ValidationRule>
