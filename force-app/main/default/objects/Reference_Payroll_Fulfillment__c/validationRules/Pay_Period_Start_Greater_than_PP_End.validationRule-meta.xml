<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pay_Period_Start_Greater_than_PP_End</fullName>
    <active>true</active>
    <errorConditionFormula>Pay_Period_Begin_Date__c  &gt;  Pay_Period_End_Date__c</errorConditionFormula>
    <errorMessage>You cannot have a Pay Period Begin Date greater than the Pay Period End Date</errorMessage>
</ValidationRule>
