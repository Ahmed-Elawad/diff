    <columns>OPPORTUNITY.CLOSE_DATE</columns>
    <columns>Reason_Not_Sold__c</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>OPPORTUNITY.LEAD_SOURCE</columns>
    <columns>Referring_Employee__c</columns>
    <columns>Referral_Contact__c</columns>
    <columns>Employees_Paid_Per_Payroll__c</columns>
    <columns>Frequency__c</columns>
    <columns>Primary_Competition__c</columns>
    <columns>Local_Competitors__c</columns>
    <filterScope>Mine</filterScope>
    <filters>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <operation>contains</operation>
        <value>Not Sold,Lost,No Decision,Disqualified,Rescinded</value>
    </filters>
    <filters>
        <field>Reason_Not_Sold__c</field>
        <operation>notContain</operation>
        <value>Cleanup,Duplicate Opportunity</value>
