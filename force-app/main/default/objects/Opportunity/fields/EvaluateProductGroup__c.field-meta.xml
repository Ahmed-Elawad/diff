<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EvaluateProductGroup__c</fullName>
    <businessStatus>Active</businessStatus>
    <formula>OR(  EvaluateProductGroupOverride__c 
    ,AND( NOT(ISBLANK(AdditionalProductsOfInterest__c))
       , ReferralLookup__r.Referral_Partner__r.Evaluate_Product_Of_Interest__c ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Do we evaluate the Additional Products of Interest field?</inlineHelpText>
    <label>Evaluate Product Group</label>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
