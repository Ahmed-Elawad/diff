<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ROE_Complete_or_Returned</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( ISBLANK(ROE_Completed_Date__c )) &amp;&amp;
NOT( ISBLANK(ROE_Returned_Date__c  ))</errorConditionFormula>
