<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Returned_equals_NO</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Check_Returned__c,&quot;NO&quot;),
ISPICKVAL(Reason_for_V_R__c,&quot;&quot;))</errorConditionFormula>
    <errorDisplayField>Reason_for_V_R__c</errorDisplayField>
    <errorMessage>Reason for V&amp;R is required.</errorMessage>
</ValidationRule>
