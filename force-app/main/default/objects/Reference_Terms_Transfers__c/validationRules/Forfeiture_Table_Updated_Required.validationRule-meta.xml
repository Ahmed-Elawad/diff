<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Forfeiture_Table_Updated_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISNULL( Forfeiture_Trade_Date__c)),
 Forfeiture_Table_Updated__c = False)</errorConditionFormula>
    <errorDisplayField>Forfeiture_Table_Updated__c</errorDisplayField>
    <errorMessage>If Forfeiture Trade Date is filled out, Forfeiture Table Updated must be checked.</errorMessage>
</ValidationRule>
