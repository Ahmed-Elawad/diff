<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Go_live_date_needs_to_be_populated</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
Go_Live__c = True,
ISBLANK(Go_Live_Date__c))</errorConditionFormula>
    <errorDisplayField>Go_Live_Date__c</errorDisplayField>
    <errorMessage>Please add Go live date</errorMessage>
</ValidationRule>
