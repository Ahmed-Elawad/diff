<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ProductUtilization__c</fullName>
    <businessStatus>Active</businessStatus>
    <formula>(IF(ISPICKVAL(X401k__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(FSA__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(SUI__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(InitialSafetyContact__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(HRO__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(COBRA__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(Handbook__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(FedStatePosters__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(EAP__c,&quot;Activated&quot;),1,0)
+IF(ISPICKVAL(Paychex_Learning_Center__c ,&quot;Activated&quot;),1,0)
) / 10</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Reflects % of products Activated</inlineHelpText>
    <label>Product Utilization</label>
    <precision>18</precision>
