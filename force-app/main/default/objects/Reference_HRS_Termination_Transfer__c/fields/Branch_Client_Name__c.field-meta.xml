<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Branch_Client_Name__c</fullName>
    <businessStatus>Active</businessStatus>
    <description>This field is used in a conga document using Conga Template called &apos;ASO Large Clients Lost&apos;. If the B/C name starts with 400s then it should say Preview ASO. If the B/C starts with 700s it should read Flex PEO. All other clients should be Flex ASO.</description>
    <externalId>false</externalId>
    <formula>IF(LEFT(Branch_Client_Number__c, 1)=&apos;4&apos;, &apos;Preview ASO&apos;, IF(LEFT(Branch_Client_Number__c, 1)=&apos;7&apos;, &apos;Flex PEO&apos;, &apos;Flex ASO&apos;) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Branch Client Name</label>
    <required>false</required>
    <securityClassification>Internal</securityClassification>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
