<apex:component allowDml="true" controller="OnboardingController">
    <div class="slds-form slds-form_stacked">
        <div class="slds-form-element slds-size_1-of-1">
            <apex:actionRegion >
            <apex:pageMessages id="msg" rendered="{!displayMessages}" escape="false"/>
            <label class="label1"><abbr class="slds-required">* </abbr>Product: </label>
            <apex:inputField value="{!onboardingRecord.Product__c}">
                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayPOP2
                                                                                    ,displayMPSC
                                                                                    ,displayExistHandbook2
                                                                                    ,displayExistSafety2
                                                                                    ,displayPOPInformation2
                                                                                    ,displayAdditional2
                                                                                    ,displayOnboardingInformation2
                                                                                    ,displayOnboarding2
                                                                                    ,displayOnboarding3
                                                                                    ,displayTaxPay2
                                                                                    ,displayHRAdmin2
                                                                                    ,displayProductInformation2
                                                                                    ,displayButton2
                                                                                    ,displayExistInfo4
                                                                                    ,displayFSSInfo2
                                                                                    ,displayTaxId2"/>
            </apex:inputField>
                </apex:actionRegion>
        </div>
        <div class="slds-form-element slds-size_1-of-1">
             <apex:actionRegion >
            <label class="label1"><abbr class="slds-required">* </abbr>Client Type: </label>
            <apex:inputField value="{!onboardingRecord.New_Existing__c}">
                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayAdditional2,displayExistInfo4"/>
            </apex:inputField>
            </apex:actionRegion>
        </div>
    </div>
    <apex:outputPanel id="displayProductInformation2">
        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c != '', 'true', 'false')}">
            <br />
            <div class="slds-box slds-box_xx-small black_background">
                <label class="header1"><b>Client Product Profile</b></label>
                <div class="slds-box brand_background">
                    <apex:outputPanel id="displayTaxPay2">
                        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c != '' 
                                                     && onboardingRecord.Product__c != 'PESS' 
                                                     && onboardingRecord.Product__c != 'HR Solutions for Non-Payroll', 'true', 'false')}">
                            <apex:inputField value="{!onboardingRecord.Taxpay__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displaySupportingDocumentation2"/>
                            </apex:inputField><label class="label1"> Currently using Taxpay services?</label><div/>
                            <apex:outputPanel id="displaySupportingDocumentation2">
                                <apex:actionRegion rendered="{!IF(onboardingRecord.Taxpay__c, 'true', 'false')}">
                                    <apex:inputField value="{!onboardingRecord.Supporting_Documentation__c}">
                                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                                    </apex:inputField>
                                    <label class="label1">Select if you have supporting documentation</label><div/><div/>
                                </apex:actionRegion>
                            </apex:outputPanel>
                        </apex:actionRegion>
                    </apex:outputPanel>
                    <apex:outputPanel id="displayPOP2">
                        <apex:actionRegion rendered="{!IF(!onboardingOpp.Account.Any_Pop__c &&(onboardingRecord.Product__c == 'HR Solutions-Flex' || onboardingRecord.Product__c == 'HR Solutions-Preview'), 'true', 'false')}">
                            <apex:inputField value="{!onboardingRecord.POP__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayPOPInformation2,displayButton2"/>
                            </apex:inputField>
                            <label class="label1">Will client be offering POP?</label><div/>
                        </apex:actionRegion>
                    </apex:outputPanel>
                    <apex:inputField value="{!onboardingRecord.PESS__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayPESSInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Will client be adding PESS?</label><div/>
                    <apex:outputPanel id="displayOnboarding2">
                        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c == 'HR Solutions-Flex', 'true', 'false')}">
                            <apex:inputField value="{!onboardingRecord.Add_Onboarding__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                            </apex:inputField>
                            <label class="label1">Will client be adding Onboarding?</label><div/>
                        </apex:actionRegion>
                    </apex:outputPanel>
                    
                    <apex:outputPanel id="displayOnboarding3">
                        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c != '' 
                                                     && (onboardingRecord.Product__c == 'HR Solutions-Flex' 
                                                     || onboardingRecord.Product__c == 'Paychex HR Essentials'), 'true', 'false')}">
                            <apex:inputField value="{!onboardingRecord.Spanish__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation3,displayButton2"/>
                            </apex:inputField>
                            <label class="label1">Does your Employee Handbook require Spanish Translation?</label><div/>
                        </apex:actionRegion>
                    </apex:outputPanel>
                    
                </div>
            </div>
        </apex:actionRegion>
    </apex:outputPanel>
    <apex:outputPanel id="displayPOPInformation2">
        <apex:actionRegion rendered="{!IF((onboardingRecord.Product__c == 'HR Solutions-Flex'|| onboardingRecord.Product__c == 'HR Solutions-Preview') 
                                     && onboardingRecord.Pop__c, 'true', 'false')}">
            <br />
            <div class="slds-box slds-box_xx-small black_background">
                <label class="header1"><b>POP Information</b> 
                    <font color = "red"> -Select at least one option:</font></label>
                <div class="slds-box brand_background">
                    <apex:inputField value="{!onboardingRecord.Dental__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Dental</label><div/>
                    <apex:inputField value="{!onboardingRecord.Group_Term_Life__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Group Term Life</label><div/>
                    <apex:inputField value="{!onboardingRecord.Short_Term_Disability__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Short Term Disability</label><div/>
                    <apex:inputField value="{!onboardingRecord.Core_Medical__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Core Medical</label><div/>
                    <apex:inputField value="{!onboardingRecord.Vision__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Vision</label><div/>
                    <apex:inputField value="{!onboardingRecord.Accidental_Death__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Accidental Death</label><div/>
                    <apex:inputField value="{!onboardingRecord.Long_Term_Disability__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayOnboardingInformation2,displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Long Term Disability</label><div/>
                </div>
            </div>
        </apex:actionRegion>
    </apex:outputPanel>
    <apex:outputPanel id="displayPESSInformation2">
        <apex:actionRegion rendered="{!IF(onboardingRecord.PESS__c, 'true', 'false')}">
            <br />
            <div class="slds-box slds-box_xx-small black_background">
                <label class="header1"><b>PESS Information</b><font color = "red"> -Select PESS options</font></label>
                <div class="slds-box brand_background">
                    <apex:inputField value="{!onboardingRecord.PESS_Background_Screening__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                    </apex:inputField>
                    <label class="label1">International Background Screening</label><div/>
                    <apex:inputField value="{!onboardingRecord.PESS_Drug_Testing__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Drug Testing</label><div/>
                    <apex:inputField value="{!onboardingRecord.PESS_I9_Verification__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                    </apex:inputField>
                    <label class="label1">I-9 Verification</label><div/>
                    <apex:inputField value="{!onboardingRecord.PESS_Motor_Vehicle_Report__c}">
                        <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                    </apex:inputField>
                    <label class="label1">Motor Vehicle Report</label><div/>
                </div>
            </div>
        </apex:actionRegion>
    </apex:outputPanel>
    <apex:outputPanel id="displayOnboardingInformation2">
        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c == 'HR Solutions-Flex' && onboardingRecord.Add_Onboarding__c, 'true', 'false')}">
            <br />
            <div class="slds-box slds-box_xx-small black_background">
                <p  class="header1"><b>Onboarding Information</b>
                    <font color = "red"> -Complete all onboarding information below:</font></p>
                <div class="slds-box brand_background">
                    <div class="slds-grid slds-wrap">
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Onboarding contact:</label><br/>
                            <apex:selectList styleClass="textBox1" size="1" value="{!onboardingRecord.Onboarding_Contact__c}">
                                <apex:selectOptions value="{!Contacts}"/>
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:selectList>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Onboarding accounts payable contact:</label><br/>
                            <apex:selectList styleClass="textBox1" size="1" value="{!onboardingRecord.Accounts_Payable_Contact__c}">
                                <apex:selectOptions value="{!Contacts}"/>
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:selectList>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Onboarding anticipated start date:</label><br/>
                            <apex:inputField style="width: 175px;" value="{!onboardingRecord.Anticipated_Start_Date__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                    </div>
                </div>
            </div>
        </apex:actionRegion>
    </apex:outputPanel>
    <apex:outputPanel id="displayContactInfo">
		<div class="slds-box slds-box_xx-small black_background">
			<label  class="header1"><b>Contact Information</b></label>
			<div class="slds-box brand_background">
				<div class="slds-grid slds-wrap">
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<label class="label1"><abbr class="slds-required">* </abbr>Authorized officer's name:</label><br/>
						<apex:actionRegion >
							<apex:selectList styleClass="textBox1" size="1" value="{!selectedAuthorizedContact}">
								<apex:selectOptions value="{!docDeskContacts}"/>
								<apex:actionSupport event="onchange" action="{!getSelectedAuthorizedDocDeskContact}" reRender="authorized"/>
							</apex:selectList><br/>
						</apex:actionRegion>
					</div>
                    <apex:actionRegion >
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<label class="label1"><abbr class="slds-required">* </abbr>Authorized officer's title:</label><br/>
						<apex:inputField style="width: 175px;" value="{!onboardingRecord.Authorized_Officer_Title__c}">
							<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
						</apex:inputField>
					</div>
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<apex:outputPanel id="authorized">
							<label class="label1"><abbr class="slds-required">* </abbr>Authorized officer's phone:</label><br/>
							<apex:inputField style="width: 175px;" value="{!onboardingRecord.Authorized_Officer_Phone__c}">
								<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
							</apex:inputField>
							<br/>
							<label class="label1"><abbr class="slds-required">* </abbr>Authorized officer's email:</label><br/>
							<apex:inputField style="width: 175px;" value="{!onboardingRecord.Authorized_Officer_Email__c}">
								<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
							</apex:inputField>
							<br/>
						</apex:outputPanel>
					</div>
					
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<label class="label1"><abbr class="slds-required">* </abbr>Is HR contact the same as Authorized Officer?:</label><br/>
						<apex:selectList styleClass="textBox1" size="1" value="{!onboardingRecord.Authorized_Off_is_HR_Contact__c}">
							<apex:selectOptions value="{!authorizedOffOptions}"/>
							<apex:actionSupport event="onchange" action="{!Validate}" reRender="hrContactName,hrContact"/>
						</apex:selectList>
                        
					</div>
					</apex:actionRegion>
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<apex:outputPanel Id="hrContactName">
							<apex:actionRegion rendered="{!IF(onboardingRecord.Authorized_Off_is_HR_Contact__c == 'No' || onboardingRecord.Authorized_Off_is_HR_Contact__c == '', true, false)}">
								<label class="label1"><abbr class="slds-required">* </abbr>HR Contact Name:</label><br/>
								<apex:inputField style="width: 175px;" value="{!onboardingRecord.HR_Contact_Name__c}">
									<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
								</apex:inputField>
							</apex:actionRegion>
						</apex:outputPanel>
					</div>
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<apex:outputPanel id="hrContact">
							<apex:actionRegion rendered="{!IF(onboardingRecord.Authorized_Off_is_HR_Contact__c == 'No' || onboardingRecord.Authorized_Off_is_HR_Contact__c == '', true, false)}">
								<label class="label1">HR Contact Title:</label><br/>
								<apex:inputField style="width: 175px;" value="{!onboardingRecord.HR_Contact_Title__c}">
									<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
								</apex:inputField>
                                <br/>
							<!--</apex:actionRegion>
						</apex:outputPanel>
					</div>
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<apex:outputPanel id="hrContact">
							<apex:actionRegion rendered="{!IF(onboardingRecord.Authorized_Off_is_HR_Contact__c == 'No' || onboardingRecord.Authorized_Off_is_HR_Contact__c == '', true, false)}">-->
								<label class="label1"><abbr class="slds-required">* </abbr>HR Contact Phone:</label><br/>
								<apex:inputField style="width: 175px;" value="{!onboardingRecord.HR_Contact_Phone__c}">
									<apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
								</apex:inputField>
							</apex:actionRegion>
						</apex:outputPanel>
						<br/>
					</div>
                    <apex:actionRegion >
					<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
						<label class="label1">Contact Notes:</label><br/>
						<apex:inputField styleClass="textBox1" value="{!onboardingRecord.Contact_Notes__c}">
                            <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                        </apex:inputField>
					</div>
                    </apex:actionRegion>
				</div>   
			</div>
		</div>
	</apex:outputPanel>
    <apex:outputPanel id="displayAdditional2">
        <apex:actionRegion rendered="{!IF(onboardingRecord.Product__c != '', 'true', 'false')}">
            <br />
            <div class="slds-box slds-box_xx-small black_background">
                <label class="header1"><b>Paychex Information</b></label>
                <div class="slds-box brand_background">
                    <div class="slds-grid slds-wrap">
                        <apex:actionRegion >
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Payroll Platform:</label><br/>
                            <apex:inputField styleClass="textBox1" style="slds-text-heading_medium" value="{!onboardingOpp.Payroll_Platform__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Client Email Address:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingOpp.Account.Email__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Company Contact:</label><br/>
                            <apex:selectList styleClass="textBox1" size="1" value="{!onboardingRecord.Company_Contact__c}">
                                <apex:selectOptions value="{!Contacts}"/>
                                <apex:actionSupport event="onchange" action="{!updateContacts}" reRender="displayButton2,displayCompanyContact2,displayHRAdmin2"/>
                            </apex:selectList>
                            <br/><apex:inputField value="{!onboardingRecord.IsCompanyContact__c}">
                            <apex:actionSupport event="onchange" action="{!updateContacts}" reRender="displayButton2,displayCompanyContact2,displayHRAdmin2"/>
                            </apex:inputField>
                            <label class="slds-text-body_small">Company Contact is also the Authorized Officer</label>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Authorized Officer's Name:</label><br/>
                            <apex:selectList styleClass="textBox1" size="1" value="{!onboardingRecord.Authorize_Officer__c}">
                                <apex:selectOptions value="{!Contacts}"/>
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:selectList>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Preferred Contact Method:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Preferred_Contact_Method__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Best Time to Call:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Best_Call_Time__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Client Company IDs:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Client_Company_IDs__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>						
						<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Industry:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Industry__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
							<br/><apex:outputText ><font color = 'Red'>Note: What does this client do?</font></apex:outputText><br/>
                        </div>
						<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>How long have they been a client?:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.How_long_a_client__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Employee Headcount Status:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Employee_Headcount_Status__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
                            </apex:inputField>
                        </div>
						<div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Other Paychex products client is currently using:</label><br/>
                            <apex:inputField styleClass="textBox1" value="{!onboardingRecord.Other_Paychex_products_in_use__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayButton2"/>
