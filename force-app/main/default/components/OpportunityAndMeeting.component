<apex:component > 
    <apex:attribute name="oppty" description="The Opportunity"
        type="Opportunity" required="true" />
    <apex:attribute name="acct" description="The Account" type="Account"
        required="true" />
    <apex:attribute name="ctct" description="The Contact" type="Contact"
        required="true" />
    <apex:attribute name="isCore" description="Is this a core opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isMMS" description="Is this an MMS opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isHRS" description="Is this an HRS opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isAncillary"
        description="Is this an ancillary opportunity" type="Boolean"
        required="true" />
    <apex:attribute name="isMSP" description="Is this an MSP opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isPEO" description="Is this a PEO opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isTAA" description="Is this a TAA opportunity"
        type="Boolean" required="true" />
    
    <!-- Start Added by Saketh -->
    <!-- <apex:attribute name="isHNB" description="Is this a HNB opportunity"
		type="Boolean" required="true" /> -->
    <apex:attribute name="isPC" description="Is this a PNC opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isMS" description="Is this a MerchantService opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isESR" description="Is this a ESR opportunity"
        type="Boolean" required="true" />
    <apex:attribute name="isPBA" description="Is this a PBA opportunity"
        type="Boolean" required="true" /> 
    <apex:attribute name="isHNB" description="Is this an HNBF opportunity"
        type="Boolean" required="true" />
    <!-- End Added by Saketh -->
    
    <!-- Updated For myStaffingPro -->
    <apex:attribute name="prodName" description="The name of the product"
        type="String" required="true" />
    <apex:attribute name="salesOrg" description="The sales org of the rep"
        type="String" required="true" />
    <apex:attribute name="isSF1" description="Is this SalesForce 1"
        type="Boolean" required="false" default="false" />
    <apex:attribute name="companySelected"
        description="Has a Company been selected from the picklist?"
        type="Boolean" required="true" />
    <apex:attribute name="existingOpps"
        description="Existing Opportunities for the table"
        type="ExistingOpptyWrapper[]" required="true" default="false" />
    <apex:attribute name="storeSelectedOppty"
        description="Controller Method to Call" type="ApexPages.Action"
        required="true" default="false" />
    <apex:attribute name="createOppty"
        description="Controller Method to Call" type="Boolean" required="true"
        default="false" />
    <apex:attribute name="hasExistingOpps"
        description="If there are existing opptys, show table" type="Boolean"
        required="true"/>
    <apex:attribute name="eventSubject"
        description="Event Subject" type="String"
        required="false" default="false" />
    <apex:attribute name="eventDescription"
        description="Event Description" type="String"
        required="false" default="false" />
    <apex:attribute name="allowCreateOpp" type="Boolean"
        description="Is the user allowed to create an opportunity?"
        required="false" default="true" />
    <apex:attribute name="existingOppMsg" type="String"
        description="The title printed on the section for existing opportunities"
        required="false" default="Do You Want to Log Your EUP Against an Existing Opportunity?" />

    <style type="text/css"> 

.headerRed {font-size:15px;
    text-align: center;
    color: red;
    font-family: FGMedCon, arial;
    margin: 12px 0 6px;
    font-weight: bold;
    font-style: normal;
    line-height: 17px;
    white-space: nowrap;
}
.headerBlack {font-size:15px;
    text-align: center;
    color: blue;
    font-family: FGMedCon, arial;
    margin: 12px 0 6px;
    font-weight: bold;
    font-style: normal;
    line-height: 17px;
    white-space: nowrap;
}
