                    <apex:inputField value="{!ctct.Products__c}" required="false"/>
                     -->
                    	
                    <apex:inputField value="{!oppty.Local_Competitors__c}"	
                    id="localComp" />
                </apex:pageBlockSection>
                
                <!-- PEO Opportunity -->
                <apex:pageBlockSection rendered="{!isPEO}" id="createOpptyPEO"
                    title="New {!prodName} Opportunity" columns="2">
                    <apex:inputField value="{!oppty.name}" />
                    <apex:outputField value="{!oppty.OpportunityType__c}" />

                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />

                    <apex:inputField value="{!oppty.DM__c}" id="dmCheckbox" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Non_DM__c}" id="nonDmCheckbox" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />

																
	 
                    
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <apex:inputField value="{!oppty.Payroll_Platform__c}" required="true"/>
                    <apex:inputField value="{!oppty.Payroll_Current_Prior_Method__c}"/>
                      <apex:inputField value="{!oppty.Local_Competitors__c}"	
                   />
                    <!--  required="true" -->
                </apex:pageBlockSection>
                
				<!--  PNC, TAA, ESR, PBA Opportunity -->
                <apex:pageBlockSection rendered="{!OR(isPC, isESR, isPBA, isTAA)}" id="createOpptyPNCTAAESRPBA" title="New {!prodName} Opportunity" columns="2">
                    
                    <apex:inputField value="{!oppty.name}" />
                    <apex:inputField value="{!oppty.OpportunityType__c}" html-disabled="true"/>
                    
                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.HNBF_Sales_Type__c}" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />
                    <apex:inputFIeld value="{!oppty.Check_Date__c}" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />
                    <apex:inputField value="{!oppty.Primary_Competition__c}"
                        id="primComp" required="true" />
                    <apex:inputField value="{!oppty.Client_Referral__c}" id="refSource" />
                    <apex:inputField value="{!acct.Current_Prior_Method__c}"
                        id="CurPriorMeth" required="true" />
                    <apex:inputField value="{!oppty.Mthd_Lead_Rcvd__c}"
                        id="mthdLeadRcvd" />
                    <apex:inputField value="{!oppty.Local_Competitors__c}"
                        id="localComp" />
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <apex:inputField value="{!oppty.Auto_Create_Follow_up__c}"
                        id="autoFollowUp" />
                </apex:pageBlockSection>
				
				<!-- MerchantService Opportunity -->
                <apex:pageBlockSection rendered="{!isMS}" id="createOpptyMerchantService"
                    title="New {!prodName} Opportunity" columns="2">
                    
                    <apex:inputField value="{!oppty.name}" />
                    <apex:inputField value="{!oppty.OpportunityType__c}" html-disabled="true"/>
                    
                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.HNBF_Sales_Type__c}" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />
                    <!-- <apex:inputFIeld value="{!oppty.Check_Date__c}" /> -->
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />
                    <apex:inputField value="{!oppty.Primary_Competition__c}"
                        id="primComp" required="true" />
                    <apex:inputField value="{!oppty.Client_Referral__c}" id="refSource" />
                    <!-- <apex:inputField value="{!acct.Current_Prior_Method__c}" id="CurPriorMeth" required="true" /> -->
                    <apex:inputField value="{!oppty.Mthd_Lead_Rcvd__c}"
                        id="mthdLeadRcvd" />
                    <!-- <apex:inputField value="{!oppty.Local_Competitors__c}"
                        id="localComp" /> -->
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <apex:inputField value="{!oppty.Auto_Create_Follow_up__c}"
                        id="autoFollowUp" />
                </apex:pageBlockSection>
				
                <!--  HNB Opportunity -->
                <apex:pageBlockSection rendered="{!isHNB}" id="createOpptyHNB"
                    title="New {!prodName} Opportunity" columns="2">
                    <apex:inputField value="{!oppty.name}" />
                    <apex:inputField value="{!oppty.OpportunityType__c}" html-disabled="true"/>

                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.HNBF_Sales_Type__c}" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />

                    <apex:inputField value="{!oppty.DM__c}" id="dmCheckbox" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />

                    <apex:inputField value="{!oppty.Non_DM__c}" id="nonDmCheckbox" />
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <!-- <apex:inputField value="{!oppty.Payroll_Platform__c}" required="true"/> -->
                </apex:pageBlockSection>
                
				<!-- End Added by Saketh -->
				
                <!--  HRS Opportunity 401 products-->
                <apex:pageBlockSection rendered="{!AND(isHRS, salesOrg=='HRS')}"
                    id="createOpptyHRS_ProdsHRS" title="Products Presented" columns="3">

                    <apex:repeat value="{!$ObjectType.Opportunity.FieldSets.X401_Products}"
                        var="prod">
                        <apex:inputField value="{!oppty[prod]}" />
                    </apex:repeat>

                </apex:pageBlockSection>
                <!--
           <apex:pageBlockSection rendered="{!AND(isHRS, salesOrg=='HRS')}" id="createOpptyHRS_ProdsHRS2" columns="1" >
        
             <th>If this is a 401(k) presentation, verify that the email address of the contact is present and valid so that the Fee Disclosure
             can be sent. If it is not correct, cancel, fix the email address on the contact, and re-enter this form.
             </th>
           </apex:pageBlockSection>
            -->
                <!--  HRS Opportunity PEO/ASO products-->
                <apex:pageBlockSection rendered="{!AND(isHRS, salesOrg=='PAS')}"
                    id="createOpptyHRS_ProdsPEO" title="Products Presented" columns="3">
                    <apex:repeat value="{!$ObjectType.Opportunity.FieldSets.PEO_ASO_Products}"
                        var="prod">
                        <apex:inputField value="{!oppty[prod]}" />
                    </apex:repeat>

                </apex:pageBlockSection>

                <!--  HRS Opportunity HNBF products-->
                <apex:pageBlockSection rendered="{!AND(isHRS, salesOrg=='HNBF')}"
                    id="createOpptyHRS_ProdsHNBF" title="Products Presented"
                    columns="3">
                    <apex:repeat value="{!$ObjectType.Opportunity.FieldSets.HNBF_Products}"
                        var="prod">
                        <apex:inputField value="{!oppty[prod]}" />
                    </apex:repeat>
                </apex:pageBlockSection>
            </apex:outputPanel>
        </apex:outputPanel>
    </apex:outputPanel>
