                    columns="2"> <!-- rendered="{!AND(isCore,NOT(isAncillary))}" -->
                    <apex:inputField value="{!oppty.name}" />
                    <apex:outputField value="{!oppty.OpportunityType__c}" />
                    
                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />
                    <apex:inputFIeld value="{!oppty.Check_Date__c}" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Referral_Account__c}"
                        id="referralAccount" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />
                    <apex:inputField value="{!oppty.Primary_Competition__c}"
                        id="primComp" required="true" />
                    <apex:inputField value="{!oppty.Client_Referral__c}" id="refSource" />
                    <apex:inputField value="{!oppty.Other_Competition__c}"
                                     id="otherComp" required="false" /> 
                    <apex:inputField value="{!acct.Current_Prior_Method__c}"
                        id="CurPriorMeth" required="true" />
                    <apex:inputField value="{!oppty.Mthd_Lead_Rcvd__c}"
                        id="mthdLeadRcvd" />
                    <apex:inputField value="{!oppty.Local_Competitors__c}"
                        id="localComp" />
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <apex:inputField value="{!oppty.Auto_Create_Follow_up__c}"
                        id="autoFollowUp" />
                </apex:pageBlockSection>

                <!--  MMS Opportunity -->
                <apex:pageBlockSection rendered="{!isMMS}"
                    id="createOpptyMMS" title="New {!prodName} Opportunity" columns="2"> <!-- rendered="{!AND(isMMS,NOT(isAncillary))}" -->
                    <apex:inputField value="{!oppty.name}" />
                    <apex:outputField value="{!oppty.OpportunityType__c}" />

                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />
                    <apex:inputField value="{!acct.Current_Prior_Method__c}"
                        id="CurPriorMeth" required="true" />
					<apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.Referral_Account__c}"
                        id="referralAccount" />		
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />
                    <apex:inputField value="{!oppty.Client_Referral__c}" id="refSource" />

                    <apex:inputField value="{!oppty.Primary_Competition__c}"
                        id="primComp" required="true" />
                    <apex:inputField value="{!oppty.Other_Competition__c}"
                        id="otherComp" />

                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.Local_Competitors__c}"
                        id="localComp" />

                    <apex:inputField value="{!oppty.Core_Conversion_Reason__c}"
                        id="conConvReason" />
                    <apex:inputField value="{!oppty.Core_Branch_Client_Number__c}"
                        id="coreCltBranchNbr" />

                    <apex:inputField value="{!oppty.Auto_Create_Follow_up__c}"
                        id="autoFollowUp" />
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
				<!--	<apex:inputField value="{!oppty.Payroll_Unit__c}" />  -->

                </apex:pageBlockSection>

                <!--  MSP Opportunity -->
                <!-- Updated For myStaffingPro -->
                <apex:pageBlockSection rendered="{!isMSP}" id="createOpptyMsp"
                    title="New {!prodName} Opportunity" columns="2">
                    <apex:inputField value="{!oppty.name}" />
                    <apex:outputField value="{!oppty.OpportunityType__c}" />

                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />
                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                </apex:pageBlockSection>

                <!--  HRS Opportunity -->
                <apex:pageBlockSection rendered="{!isHRS}" id="createOpptyHRS"
                    title="New {!prodName} Opportunity" columns="2">
                    <apex:inputField value="{!oppty.name}" />
                    <apex:outputField value="{!oppty.OpportunityType__c}" />

                    <apex:inputField value="{!oppty.CloseDate}" id="closeDt" />
                    <apex:inputField value="{!oppty.LeadSource}" id="leadSource"
                        required="true" />

                    <apex:inputField value="{!oppty.DM__c}" id="dmCheckbox" />
                    <apex:inputField value="{!oppty.Level_2__c}" />
                    
                    <apex:inputField value="{!oppty.Non_DM__c}" id="nonDmCheckbox" />
                    <apex:inputField value="{!oppty.Referral_Contact__c}"
                        id="referralContact" />

                    <apex:inputField value="{!oppty.Referring_Employee__c}" />
