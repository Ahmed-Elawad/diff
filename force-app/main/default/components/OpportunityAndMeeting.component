            <apex:outputPanel rendered="{!createOppty}">
                <section class="section">
                    <h2 class="section-head">New {!prodName} Opportunity</h2>
                    <!--  Core Opportunity -->
                    <apex:outputPanel rendered="{!isCore}"
                        id="createOpptyCoreSF1" title="New {!prodName} Opportunity"> <!-- "{!AND(isCore,NOT(isAncillary))}" -->
                        <div class="form-group has-error">
                            <label class="control-label" for="OppName1">Opportunity
                                Name</label>
                            <apex:inputField type="text" styleClass="form-control"
                                value="{!oppty.name}" id="OppName1" />
                        </div>
                        <div class="form-group has-error">
                            <label class="control-label" for="closeDt1">Run-Effective
