    <apex:outputPanel id="createOpptyContainer">
        <apex:outputPanel rendered="{!NOT(isSF1)}">
        
            <apex:outputPanel rendered="{!createOppty}">
                <!--  Core Opportunity -->
                <apex:pageBlockSection rendered="{!isCore}"
