<apex:component >
    <div id="renderLightingComponent"/>
    <apex:includeLightning />
    <script>
        $Lightning.use("c:UnsubscribeEmail", function() {
            $Lightning.createComponent(
                "c:unsubscribeFromEmail",
                {},
                "renderLightingComponent",
                function(cmp) {
                    console.log("Component is created!");
                    console.log(cmp);
                });
            });
    </script>
</apex:component>