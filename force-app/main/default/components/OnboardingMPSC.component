                            <label class="label1"><abbr class="slds-required">* </abbr>Reason(s) client selected HR Essentials:</label><br/>
                            <apex:inputField styleClass="textBox1" style="slds-text-heading_medium" value="{!onboardingRecord.HRE_Reason_Selected__c}">
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayFSSButton"/>
                            </apex:inputField>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Does client have an existing handbook?</label><br/>
                            <apex:selectList style="width: 150px;" size="1" value="{!onboardingRecord.HRE_Existing__c}">
                                <apex:selectOptions value="{!BoolOption}"/>
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayFSSButton"/>
                            </apex:selectList>
                        </div>
                        <div class="slds-p-horizontal_small slds-p-bottom_medium slds-size_1-of-1 slds-medium-size_1-of-2 slds-large-size_1-of-3">
                            <label class="label1"><abbr class="slds-required">* </abbr>Does the client have an existing safety manual?</label><br/>
                            <apex:selectList style="width: 150px;" size="1" value="{!onboardingRecord.HRE_Existing_Safety_Manual__c}">
                                <apex:selectOptions value="{!BoolOption}"/>
                                <apex:actionSupport event="onchange" action="{!Validate}" reRender="displayFSSButton"/>
                            </apex:selectList>
                        </div>
                    </div>
                </div>
            </div>
        </apex:actionRegion>
    </apex:outputPanel>
    
    <br />
    <div class="slds-box slds-box_xx-small black_background">
        <div class="slds-box brand_background" align="center">
            <label class="label1">All required fields must be completed prior to submission</label><br/>
            <apex:actionRegion >
                <apex:commandButton styleClass="slds-button--neutral" value="Save Changes - Return to Opportunity" onclick="saveConfirmation()" action="{!saveChanges}" />
            </apex:actionRegion>
            <apex:outputPanel id="displayFSSButton">
                <apex:actionRegion rendered="{!IF(validRecord, 'true', 'false')}">
                    <apex:commandButton styleClass="slds-button--neutral" value="Submit New Client Profile" action="{!submitNCP}" onclick="submitConfirmation()" />
                </apex:actionRegion>
