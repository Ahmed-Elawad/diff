<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>HRS4SDirector</submitter>
        <type>roleSubordinates</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>ButtonBar</field>
        <field>Name</field>
        <field>Owner</field>
        <field>SunGard_Number__c</field>
        <field>Wire_Amount__c</field>
        <field>Wire_Line_Number__c</field>
        <field>FED_Wire_Type__c</field>
        <field>Account_Number__c</field>
        <field>ABA_Routing_Number__c</field>
        <field>Bank_Name__c</field>
        <field>Reference__c</field>
        <field>FBO__c</field>
        <field>Integrity_Link__c</field>
        <field>Paperwork_Web_Link__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Owner</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Level 2</label>
        <name>Level_2</name>
    </approvalStep>
    <emailTemplate>HRS_4s_401k_Terminations1/Approval_Waiting</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Populate_Wire_Sent_Date</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Transfer_to_Cancellations</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Email_Cancellations</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Transfer_to_Cancellations</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>Wire Approval Process Step 3</label>
    <processOrder>3</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
