<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>false</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Prospect_Client_Name__c</field>
        <field>Owner</field>
        <field>Reason_for_Exception__c</field>
        <field>Payroll_Frequency__c</field>
        <field>Revenue__c</field>
        <field>Setup_Fee__c</field>
        <field>Number_of_IDs_Formula__c</field>
        <field>Are_any_IDs_Combos__c</field>
        <field>Previous_Wages__c</field>
        <field>of_Employees_with_YTDs__c</field>
        <field>of_Active_Employees__c</field>
        <field>of_Terminated_Employees__c</field>
        <field>Are_we_loading_termed_ees__c</field>
        <field>Prior_Service_Provider__c</field>
        <field>Format_of_EE_and_Wage_Data__c</field>
        <field>Name_of_States__c</field>
        <field>Job_Costing_or_Labor_Distribution__c</field>
        <field>Interfaces_with_outside_vendor__c</field>
        <field>Have_you_engaged_SE__c</field>
        <field>Union_Dues_or_Special_Calc_Deductions__c</field>
        <field>Client_knows_GL_not_provided_on_1st_PR__c</field>
        <field>Client_knows_Flextime_not_on_1st_PR__c</field>
        <field>Other_timeclocks_SE_Engaged__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>userHierarchyField</type>
            </approver>
        </assignedApprover>
        <label>Send to DSM</label>
        <name>Send_to_DSM</name>
    </approvalStep>
    <description>Approval Process for MPSC Exception</description>
    <emailTemplate>MPSC_Emails/MPSC_Exception_Approval</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Update_DSM_Approval_field</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Update_DSM_Approval_Field_Rejected</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>MPSC Exception Approval</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>false</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>false</showApprovalHistory>
</ApprovalProcess>
